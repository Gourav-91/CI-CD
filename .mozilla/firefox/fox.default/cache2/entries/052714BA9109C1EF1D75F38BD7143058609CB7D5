document.writeln("<script type='text\/javascript' src='/domTT/domTT.js'><\/script>");
function sdmGetPageActions(pagepath, n_rows, start_idx, prev_idx_list, last_idx, prefix) {
    var pageactions = new Array();
    var prev_pages;
    var basepagepath = pagepath;
    if (pagepath.contains("?")) {
        basepagepath += "&";
    } else {
        basepagepath += "?";
    }
    if (prev_idx_list == "" && last_idx == "") {
         return undefined;
    }
    if (prev_idx_list == "") {
        pageactions.prev = "none";
        pageactions.first = "none";
    } else {
        pageactions.first = "document.location.href='" + basepagepath + prefix + "last_idx=A-1&" + prefix + "n_rows=" + n_rows + "'";
        prev_pages = prev_idx_list.split(",");
        if (prev_pages.length == 1) {
            pageactions.prev = pageactions.first;
        } else {
            var startAt = prev_pages[prev_pages.length-1];
            var prev_idx_new = "";
            var i;
            for (i = 0; i < prev_pages.length - 1; i++) {
                if (i > 0) {
                    prev_idx_new += ",";
                }
                prev_idx_new += prev_pages[i];
            }
            pageactions.prev = "document.location.href='" + basepagepath + prefix + "start_at=" + startAt + "&" + prefix + "n_rows=" + n_rows + "&" + prefix + "prev_idx_list=" + prev_idx_new + "'";
        }
    }
    if (last_idx == "") {
        pageactions.last = "none";
        pageactions.next = "none";
    } else {
        if (prev_idx_list != "") {
            prev_idx_list += ","
        }
        pageactions.next = "document.location.href='" + basepagepath + prefix + "prev_last=" + last_idx + "&" + prefix + "n_rows=" + n_rows + "&" + prefix + "prev_idx_list=" + prev_idx_list + start_idx + "'";
        pageactions.last = "document.location.href='" + basepagepath + prefix + "get_last=" + last_idx + "&" + prefix + "n_rows=" + n_rows + "&" + prefix + "prev_idx_list=" + prev_idx_list + start_idx + "'";
    }
    if (pageactions.first == "none" && pageactions.last == "none") {
        pageactions.showsingle = "none";
    } else {
        pageactions.showsingle = "document.location.href='" + basepagepath + prefix + "n_rows=all';";
    }
    return pageactions;
}
function popElementPrevIdx(prev_idx_list) {
    return last_item;
}
function openProblemsViewable() {
    if (ht_isDomainSP() || (ht_enableILOMSettings() == false)) {
        return false;
    }
    return true;
}
function systemLogViewable() {
    return openProblemsViewable();
}
function viewOpenProblems() {
    ht_selectTreeNode("Open_Problems");
    return false;
}
function buildDetailsPopupLink(obj_internal_name, obj_display_name, obj_path) {
    var detailsId = "details_" + obj_path + obj_internal_name;
    detailsId = detailsId.make_html_id();
    var linkStr = "<a href='#' id='" + detailsId + "' onmouseover='sdmDisplayDetailsTooltip(this, event, \"" + obj_display_name + "\", \"" + obj_path + "\", \"" + obj_internal_name + "\"); return false;'";
    linkStr += " onclick='domTT_deactivate(this.id);sdmDisplayDetails(\"" + obj_path + "\" , \"" + obj_internal_name + "\"); return false;'>Details<\/a>";
    return linkStr;
}
function walkSDMTableArray(tableAry, selectDropdown, pathRoot, colnames) {
    if (! pathRoot.endsWith("/")) {
        pathRoot += "/";
    }
    var i;
    for (i = 0; i < tableAry.length; i++) {
        var name = tableAry[i]['data']['obj_name'];
        var health = tableAry[i]['data']['health'];
        var obj_path = pathRoot + name;
        tableAry[i]['data']['obj_name'] = tableAry[i]['data']['obj_name'].replace(/_/g, " ");
        if (health) {
            tableAry[i]['data']['health_details'] = buildHealthDetailsInnerHtml(health, tableAry[i]['data']['health_details']);
        }
        tableAry[i]['data']['details'] = buildDetailsPopupLink(name, tableAry[i]['data']['obj_name'], obj_path);
        stashStore("colnames_" + name, colnames);
        if (selectDropdown) {
            var rowActions =  new Array();
            var action = tableAry[i]['meta']['action'];
            if (action && action.indexOf('return_to_service') != -1) {
                if (health.strcasecmp("offline")) {
                    rowActions.push('return_to_service');
                } else {
                    rowActions.push('prepare_to_remove');
                }
            } else if (action && action.indexOf('power_on') != -1) {
                var power_state = tableAry[i]['data']['power_state'];
                if (power_state.strcasecmp("On")) {
                    rowActions.push('power_off,power_off_force');
                } else {
                    if (action.indexOf('power_on_force') != -1) {
                        rowActions.push('power_on,power_on_force');
                    } else {
                        rowActions.push('power_on');
                    }
                }
            } else if (tableAry[i].data.requested_state) {
                var state = tableAry[i].data.requested_state;
                if (state.strcasecmp("enabled")) {
                    rowActions.push('disabled');
                } else {
                    rowActions.push('enabled');
                }
            } else if (tableAry[i].data.initiate_sp_failover != undefined) {
                rowActions.push('failover_graceful,failover_force');
            }
            if (rowActions.length > 0) {
                tableAry[i]['meta']['row_actions'] = rowActions.join(",");
            } else {
                tableAry[i]['meta']['row_actions'] = 'not_supported';
            }
        }
        var crows = tableAry[i]['data']['crows'];
        if (crows) {
            for (j = 0; j < crows.length; j++) {
                var crow_health = crows[j]['data']['health'];
                var crow_name = crows[j]['data']['obj_name'];
                var crow_obj_path = obj_path + "/" + crow_name;
                var crow_action = crows[j]['meta']['action'];
                crows[j]['data']['obj_name'] = crow_name.replace(/_/g, " ");
                var rowActions = new Array();
                if (crow_health) {
                    crows[j]['data']['health_details'] = buildHealthDetailsInnerHtml(
                        crow_health, crows[j]['data']['health_details']);
                    if (selectDropdown && crow_action && crow_action.indexOf('return_to_service') != -1) {
                        if (crow_health.strcasecmp("offline")) {
                            rowActions.push('return_to_service');
                        } else {
                            rowActions.push('prepare_to_remove');
                        }
                    }
                }
                if (selectDropdown && crow_action && crow_action.indexOf('power_on') != -1) {
                    var crow_power_state = crows[j]['data']['power_state'];
                    if (crow_power_state) {
                        if (crow_power_state.strcasecmp("On")) {
                            rowActions.push('power_off,power_off_force');
                        } else {
                            if (crow_action.indexOf('power_on_force') != -1) {
                                rowActions.push('power_on,power_on_force');
                            } else {
                                rowActions.push('power_on');
                            }
                        }
                    }
                }
                if (crows[j].data.requested_state) {
                    var crow_state = crows[j].data.requested_state;
                    crows[j]['meta']['settable'] = 'true';
                    if (crow_state.strcasecmp("enabled")) {
                        rowActions.push('disabled');
                    } else {
                        rowActions.push('enabled');
                    }
                }
                if (selectDropdown) {
                    if (rowActions.length > 0) {
                        crows[j]['meta']['row_actions'] = rowActions.join(",");
                    } else {
                        crows[j]['meta']['row_actions'] = 'not_supported';
                    }
                }
                crows[j]['data']['details'] = buildDetailsPopupLink(crow_name, crows[j]['data']['obj_name'], crow_obj_path);
                stashStore("colnames_" + crow_name, colnames);
            }
        }
    }
    writePopupDivs(tableAry, colnames, name, pathRoot);
}
function getSDMDropdownArray(data) {
    var action_added = false;
    var req_state_added = false;
    var failover_added = false;
    var dropdownArray = new Array();
    var i;
    for (i = 0; i < data.length; i++) {
        if (data[i].meta.action && action_added == false) {
            action_added = true;
            dropdownArray.push({value:'Prepare to Remove',
                                row_action_name: 'prepare_to_remove',
                                action:'setAction({property:"action", value:"prepare_to_remove", action:"Prepare to Remove", that:this});'});
            dropdownArray.push({value:'Return to Service',
                                row_action_name: 'return_to_service',
                                action:'setAction({property:"action", value:"return_to_service", action:"Return to Service", that:this});'});
        }
        if (data[i].data.requested_state && req_state_added == false) {
            req_state_added = true;
            dropdownArray.push({value:'Disable',
                                row_action_name: 'disabled',
                                action:'setAction({property:"requested_state", value:"disabled", action:"Disable", that:this});'});
            dropdownArray.push({value:'Enable',
                                row_action_name: 'enabled',
                                action:'setAction({property:"requested_state", value:"enabled", action:"Enable", that:this});'});
        }
        if (data[i].data.initiate_sp_failover != undefined && failover_added == false) {
            failover_added = true;
            dropdownArray.push({value:'Graceful Failover',
                                row_action_name: 'failover_graceful',
                                action:'setAction({property:"initiate_sp_failover", value:"true", action:"Graceful Failover", that:this});'});
            dropdownArray.push({value:'Force Failover',
                                row_action_name: 'failover_force',
                                action:'setAction({property:"initiate_sp_failover", value:"force", action:"Force Failover", that:this});'});
        }
    }
    return dropdownArray;
}
function getSDMPowerDropdownArray(data) {
    var action_added = false;
    var power_off_force_added = false;
    var dropdownArray = new Array();
    var i;
    for (i = 0; i < data.length; i++) {
        if (data[i].meta.action) {
            if (!action_added) {
                action_added = true;
                dropdownArray.push({value:'Power On',
                                    row_action_name: 'power_on',
                                    action:'setAction({action:"power_on", that:this});'});
                dropdownArray.push({value:'Power Off',
                                    row_action_name: 'power_off',
                                    action:'setAction({action:"power_off", that:this});'});
                dropdownArray.push({value:'Forcibly Power Off',
                                    row_action_name: 'power_off_force',
                                    action:'setAction({action:"power_off_force", that:this});'});
            }
            if (data[i].meta.action.indexOf('power_on_force') != -1) {
                if (!power_off_force_added) {
                    power_off_force_added = true;
                    dropdownArray.splice(1, 0, {value:'Forcibly Power On',
                                               row_action_name: 'power_on_force',
                                               action:'setAction({action:"power_on_force", that:this});'});
                }
            }
        }
    }
    return dropdownArray;
}
function wrapHealthDetail(origstring, maxwide) {
    var lines = Math.ceil(origstring.length / maxwide);
    var wrappedstr = origstring.substr(0, maxwide);
    if (lines > 1)
        wrappedstr += "...";
    return wrappedstr;
}
function sdmDisplayDetails(path, name) {
     stashStore("componentname", name);
     genericWindow({fileNm:'frameSDMDetails.asp?path=' + path,
                    buttonArray:'Close', height:590, width:590});
}
function handleHealthStatus(health) {
    if (!health) {
        health = "";
    }
    var healthHTML = health.replace(/ /g,"&nbsp;");
    document.write("<span class='tableCellNoWrap'>");
    if (health.strcasecmp("okay") || health.strcasecmp("ok")) {
        document.write(showTransparentPngHtml({imgPath:'/Images/oracle/icons/okay_icon.png', width:12, height:13, alt:'', valign:'text-top'}) + "&nbsp;" + healthHTML);
    } else if (health.strcasecmp("service required") || health.strcasecmp("disabled (service required)")) {
        document.write(showTransparentPngHtml({imgPath:'/Images/oracle/icons/error_icon.png', width:12, height:13, alt:'', valign:'text-top'}) + "&nbsp;" + healthHTML + "\n");
    } else if (health.strcasecmp("warning")  || health.strcasecmp("not available") || health.strcasecmp("offline") || health.strcasecmp("degraded") || health.strcasecmp("disabled")) {
        document.write(showTransparentPngHtml({imgPath:'/Images/oracle/icons/warning_icon.png', width:13, height:13, alt:'', valign:'text-top'}) + "&nbsp;" + healthHTML + "\n");
    } else {
        document.write(showTransparentPngHtml({imgPath:'/Images/oracle/icons/warning_icon.png', width:13, height:13, alt:'', valign:'text-top'}) + "&nbsp;Unknown");
    }
    document.write("</span>");
}
function buildHealthDetailsInnerHtml(health, health_details) {
    if (!health) {
        health = "";
    }
    if (health.strcasecmp("okay") || health.strcasecmp("ok")) {
        return "<img src='/Images/sun/2.2/table/empty_table_cell.gif' alt='Not Applicable' />";
    }
    var msg = DefaultInnerHtmlNA(health_details).linkify();
    if (openProblemsViewable()) {
        if (! health.strcasecmp("not available") && ! health.strcasecmp("degraded") && ! health.strcasecmp("disabled") && ! health.strcasecmp("offline")) {
            msg += "<br \/>See the <a href='#' onClick='return viewOpenProblems();'>Open Problems</a> page for more information.";
        }
    }
    return msg;
}
function sdmDisplayDetailsTooltip(in_this, event, name, obj_path, id) {
    var ttBase = obj_path.make_html_id();
    var ttId = "ttDetails" + ttBase;
    var divId = "ttDiv" + ttBase;
    domTT_activate(in_this, event, "id", ttId, "caption", name + " Details<hr style='margin-top:2px;margin-bottom:2px' \/>", "content", getElement(divId), "direction", "southwest", "styleClass", "tooltip", "type", "velcro", "inframe", false);
    return false;
}
function sdmDisplayHealthTooltip(in_this, event, name, health, healthDetails) {
    if (!health) {
        health = "";
    }
    var ttName = name.replace(/ /g, "_") + "_health";
    var captionStr = name + " Status: " + health;
    var contentStr = "<p>" + healthDetails + "</p>";
    if (health.strcasecmp("not available")) {
        contentStr = contentStr.linkify();
    } else if (openProblemsViewable()) {
        contentStr += "<p>View <a onClick='return viewOpenProblems();' title='View Open Problems' href='#'>Open Problems</a> for more details.</p>";
    }
    domTT_activate(in_this, event, "id", ttName, "caption", captionStr, "content", contentStr, "styleClass", "tooltip", "type", "velcro", "inframe", false);
    return false;
}
function sdmDisplayTooltip(in_this, event, name, value, detail) {
    if (!value) {
        value = "";
    }
    if (!detail) {
        detail = "";
    }
    var ttName   = name.replace(/ /g, "_") + "_detail";
    var nameStr  = name.replace(/ /g, "&nbsp;");
    var valueStr = value.replace(/ /g, "&nbsp;");
    var captionStr = nameStr + ":&nbsp;" + valueStr;
    var contentStr = "<p>" + detail + "<\/p>";
    contentStr = contentStr.linkify();
    domTT_activate(in_this, event, "id", ttName, "caption", captionStr, "content", contentStr, "styleClass", "tooltip", "type", "velcro", "inframe", false);
    return false;
}
function writePropertyNameValueRows(dataArray, colnames) {
    var name, val;
    if (! dataArray) {
        return;
    }
    for (var j in dataArray) {
        if (j == "row_index" || j == "details" || j == "action" ||
            j == "row_actions" || j == "crows" || j == "obj_name" ||
            j.match(/_\d$/)) {
            continue;
        }
        document.write("<tr>\n");
        if (colnames && colnames[j]) {
            name = colnames[j];
        } else {
            name = make_pretty_propname(j);
        }
        name = name.replace(/ /g, "&nbsp;");
        val = dataArray[j];
        if (val == "") {
            val = "<i>(none)<\/i>";
        }
        document.write("<td valign='top'><span class='tooltipSubtitle'><label for='" + j + "'>" + name + ":<\/label><\/span><\/td>\n");
        document.write("<td valign='top'>" + val + "<\/td>\n");
        document.write("<\/tr>\n");
    }
}
function writePopupDivs(tableAry, colnames, name, path) {
    document.write("<div style='display:none'>\n");
    var i;
    for (i = 0; i < tableAry.length; i++) {
        var ttBase = path + tableAry[i]['data']['obj_name'];
        ttBase = ttBase.make_html_id();
        var divId = "ttDiv" + ttBase;
        document.write("<div id='" + divId + "' style='font-weight:normal'>");
        document.write("<table border='0' cellspacing='0' width='100%' role='presentation'>\n");
        writePropertyNameValueRows(tableAry[i].data, colnames);
        document.write("<\/table>\n");
        document.write("<\/div>\n");
        var crows = tableAry[i]['data']['crows'];
        if (crows) {
            for (j = 0; j < crows.length; j++) {
                 var ttCBase = ttBase + "_" + crows[j]['data']['obj_name'].make_html_id()
                 var crow_divId = "ttDiv" + ttCBase;
                 document.write("<div id='" + crow_divId + "' style='font-weight:normal'>");
                 document.write("<table border='0' cellspacing='0' width='100%' role='presentation'>\n");
                 writePropertyNameValueRows(crows[j].data, colnames);
                 document.write("<\/table>\n");
                 document.write("<\/div>\n");
           }
       }
    }
    document.write("<\/div>\n");
}
function sdmHealthSubsystemToLink(subsystemName)
{
    var treeframe = parent.treeFrame;
    var subsystemKey;
    switch (subsystemName) {
        case "Memory":
        case "Power":
        case "Cooling":
        case "Storage":
        case "Networking":
        case "Firmware":
        case "Processors":
            subsystemKey = subsystemName;
            break;
        case "Blade":
            subsystemKey = "Blades";
            break;
        case "IO Module":
            subsystemKey = "I_O_Modules";
            break;
        case "Domain Configuration Unit":
            subsystemKey = "DCUs";
            break;
        case "CPU Module":
            subsystemKey = "CPU_Modules";
            break;
        case "Other Removable Devices":
            subsystemKey = "Other_Removable_Devices";
            break;
    }
    if (subsystemKey) {
        return "<a onClick='ht_selectTreeNode(\"System_Information\", \"" + subsystemKey + "\");' href='#' title='View " + subsystemName + "'>" + subsystemName + "<\/a>";
    }
    return subsystemName;
}
