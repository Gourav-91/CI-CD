var info = new Array();
function AddInfo(row, name, value) {
    if (typeof(value) != "undefined") {
        info.push({meta:{settable:'false'}, data:{row_index:row, name:name, value:value}});
    }
}
function AddInfoDetail(row, name, value, detail) {
    if (typeof(name) != "undefined"  &&  typeof(value) != "undefined") {
        var valueStr = value;
        if (typeof(detail) != "undefined"  &&  ! detail.strcasecmp("-")) {
            var mouseoverStr = "sdmDisplayTooltip(this, event, \"" + name + "\", \"" + value + "\", \"" + detail + "\"); return false;";
            var a_id = name.make_html_id().toLowerCase() + "_lnk";
            var valueStr = "<a id='" + a_id + "' href='#' onmouseover='" + mouseoverStr + "' onClick='" + mouseoverStr + "'>" + value + "<\/a>\n";
        }
        info.push({meta:{settable:'false'}, data:{row_index:row, name:name, value:valueStr}});
    }
}
