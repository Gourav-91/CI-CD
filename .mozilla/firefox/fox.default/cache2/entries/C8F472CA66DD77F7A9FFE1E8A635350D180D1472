<script type="text/javascript">
function healthToIcon(caption, index, onClickAction, classStr)
{
    var healthStr, healthIcon, healthVal, health, mouseoverStr;
    if (typeof(index) == "undefined") {
        health = "Service Required";
        caption = "Sample";
        onClickAction = "return false;";
        classStr = "";
        mouseoverStr = "";
        healthVal = health.replace(/ /g, "&nbsp;");
    } else {
        if (sysinfo == undefined || sysinfo[index] == undefined ||
            sysinfo[index].data == undefined || sysinfo[index].data.health == undefined) {
            return "UNKNOWN";
        }
        classStr = DefaultInit(classStr, "");
        health = sysinfo[index].data.health;
        healthVal = health.replace(/ /g, "&nbsp;");
        if (health.strcasecmp("okay") || health.strcasecmp("ok")) {
            healthStr = showTransparentPngHtml({imgPath:'/Images/oracle/icons/okay_icon.png', width:12, height:13, alt:'', valign:'text-top', classname:'healthIcon'}) + healthVal + "\n";
            return healthStr;
        }
        mouseoverStr = "sdmDisplayHealthTooltip(this, event, \"" + caption + "\", sysinfo[" + index + "].data.health, sysinfo[" + index + "].data.health_details); return false;";
    }
    if (classStr) {
        classStr = " class='" + classStr + "'";
    }
    var a_icon_id = caption.make_html_id().toLowerCase() + "_icon";
    if (health.strcasecmp("service required")) {
        healthIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/error_icon.png', width:12, height:13, alt:'', valign:'text-top', id:a_icon_id, onmouseover:mouseoverStr, onclick:onClickAction, classname:'healthIcon'});
    } else if (health.strcasecmp("warning")) {
        healthIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/warning_icon.png', width:13, height:13, alt:'', valign:'text-top', id:a_icon_id, onmouseover:mouseoverStr, onclick:onClickAction, classname:'warningIcon'});
    } else if (health.strcasecmp("not available")) {
        healthIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/warning_icon.png', width:13, height:13, alt:'', valign:'text-top', id:a_icon_id, onmouseover:mouseoverStr, onclick:onClickAction, classname:'warningIcon'});
    } else {
        return "UNKNOWN";
    }
    var a_id = caption.make_html_id().toLowerCase() + "_lnk";
    healthStr = healthIcon + "<a id='" + a_id + "' href='#' onmouseover='" + mouseoverStr + "' onClick='" + onClickAction + "'" + classStr + ">" + healthVal + "<\/a>\n";
    if (index != undefined && sysinfo[index].data.health_details) {
        healthStr += "<div class='hiddenContent'>";
        healthStr += sysinfo[index].data.health_details.linkify();
        if (openProblemsViewable()) {
            healthStr += "<p>View <a onClick='return viewOpenProblems();' href='#'>Open Problems<\/a> for more details.<\/p>";
        }
        healthStr += "<\/div>";
    }
    return healthStr;
}
var index;
var serverstatus = new Array();
var emptyPair = {name:'', value:''};
index = sysinfoIndex.Blades;
if (typeof(index) == "number") {
    var bladesOnClick = "ht_selectTreeNode(\"System_Information\", \"Blades\"); return false;";
    var bladesDetails = new Array(emptyPair);
    var bladesInvValue = FormatInstalledMax(sysinfo[index].data.installed_blades, sysinfo[index].data.max_blades);
    var bladesInv = new Array({name:'Installed Blades:', value:bladesInvValue}, emptyPair);
    var bladesSpan = "<a onClick='" + procOnClick + "' href='#' title='Blades'>Blades<\/a>";
    var bladesHealth = healthToIcon("Blades", index, bladesOnClick);
    serverstatus.push({meta:{settable:'false'}, data:{row_index:'blades', items:bladesSpan, status:bladesHealth, details:bladesDetails, inventory:bladesInv}});
}
index = sysinfoIndex.ComputeNodes;
