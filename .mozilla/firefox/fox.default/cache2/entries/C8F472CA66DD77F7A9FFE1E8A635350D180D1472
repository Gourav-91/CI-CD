        iomodulesInv.push({name:'Installed FEMs',
            value:FormatInstalledMax(sysinfo[index].data.installed_fems, sysinfo[index].data.max_fems)});
    }
    if (typeof(sysinfo[index].data.installed_rems) != "undefined") {
