<script type="text/javascript">
var values = new Array();
var retries = 0;
var expected_power_state;
var handleSet = function(error, text) {
    if (error == false && text == "OK") {
        if (typeof(this.data) == "object") {
            makeBall(this.data.val, this.data.type);
        }
    } else {
        alert("Set failed: " + text);
    }
}

handlePowerGet = function(error, resp_val)
{
    var state_val = parseAjaxResponse(error, resp_val);
    if (!state_val || typeof(state_val) != "object" || typeof(state_val[0]) != "string" || state_val[0] == "") {
        alert("Could not obtain current power state");
        return;
    }
    var curr_state = state_val[0];
    if (expected_power_state.strcasecmp("update_only")) {
        expected_power_state = sysinfoData.power_state = curr_state;
        makeBall(curr_state, "Power");
        return;
    }
    if (curr_state.strcasecmp(expected_power_state)) {
        sysinfoData.power_state = curr_state;
        makeBall(curr_state, "Power");
        document.getElementById("power_status").innerHTML = "";
        retries = 0;
    } else {
        setButtonDisable("button_Power");
        document.getElementById("power_status").innerHTML = "<font size='-15%'>Power " + expected_power_state + " In Progress...<\/font>";
        if (retries < 20) {
            setTimeout("getPowerState()", 10000);
        } else {
            alert("Unable to confirm that the power state was changed. Please check the status of the host OS or its settings.");
            setButtonEnable("button_Power");
            document.getElementById("power_status").innerHTML = "";
            retries = 0;
        }
    }
}
getPowerState = function()
{
    retries++;
    var getpower = new htAjax({params:"cmd=luapiGet&0=/System/power_state", resp_func:handlePowerGet});
    getpower.send();
}
handlePowerSet = function(error, text)
{
    if (error == false && text == "OK") {
        retries = 0;
        getPowerState();
    } else {
        alert("Set failed: " + text);
        expected_power_state = "update_only";
        getPowerState();
    }
}


doSet = function(type, val)
{
    var path;
    var resp_func;

    if (type == "Power") {
        resp_func = handlePowerSet;
        path = "/System/action";
        if (val.strcasecmp("power_off")) {
            expected_power_state = "Off";
        } else if (val.strcasecmp("power_on")) {
            expected_power_state = "On";
        }
    } else


    if (type == "Locator") {
        resp_func = handleSet;
        path = "/System/locator_indicator";
    } else {
        alert("Oops invalid type: " + type);
        return;
    }
    var setobj = new htAjax({params:"cmd=luapiTestAndSet&0=" + path + "&1=" + val, resp_func:resp_func, data:{type:type, val:val}});
    setobj.send();
}
summaryPageAction = function(type, state)
{
    var txt, path, val, systype;
    if (type == "Locator") {
        if (state.strcasecmp("on")) {
            val = "off";
            txt = "The location LED will be set to off. Click OK to continue.";
        } else {
            val = "on"
            txt = "The location LED will be set to on. Click OK to continue.";
        }
        if (confirm(txt)) {
            doSet(type, val);
        }

    } else if (type == "Firmware") {
        ht_selectTreeNode("ILOM_Administration", "Maintenance");
        return;


    } else if (type == "Power") {

        systype = "host";

        if (state.strcasecmp("ON")) {
            val = "power_off";
            txt = "The " + systype + " power will be set to off. Click OK to continue.";
        } else {
            val = "power_on";
            txt = "The " + systype + " power will be set to on. Click OK to continue.";
        }
        if (confirm(txt)) {
            doSet(type, val);
        }


    } else {
        return;
    }
}
makeBall = function(state, type) {
    var showButton = false;
    if (state == undefined) {
        state = "UNKNOWN";
    }
    var display_state = state;
    state = state.toUpperCase();

    if (type == "Power") {
        if (state == "POWER_ON") {
            sysinfoData.power_state = "On";
            state = "ON";
        } else if (state == "POWER_OFF") {
            sysinfoData.power_state = "Off";
            state = "OFF";
        }


        {
            showButton = true;
        }

    } else

    if (type == "Locator") {
        showButton = true;
    } else {
        alert("OOPS ball type:" + type);
    }
    if (state == "ON") {
        document.getElementById(type + "_ball").innerHTML=showTransparentPngHtml({imgPath:'/Images/ilom/status/PWR-on.png', width:16, height:16, alt:'', valign:'text-top', classname:'healthIcon'}) + state;
        if (showButton) {
            document.getElementById(type + "_button").innerHTML="<input class='button' type='submit' name='" + type + "_" + state + "'" +
                " value='Turn Off' " + "title='Turn " + type +
                " Off' style='width:80px;margin-top:5px;margin-bottom:5px;margin-left:13px;' onclick='summaryPageAction(\"" + type + "\", \"" + state + "\");'>";
        } else {
                document.getElementById(type + "_button").innerHTML="";
        }
    } else if (state == "OFF") {
        document.getElementById(type + "_ball").innerHTML=showTransparentPngHtml({imgPath:'/Images/ilom/status/PWR-off.png', width:16, height:16, alt:'', valign:'text-top', classname:'healthIcon'}) + state;
        if (showButton) {
            document.getElementById(type + "_button").innerHTML="<input class='button' type='submit' name='" + type + "_" + state + "'" +
                "value='Turn On' title='Turn " + type + " On' style='width:80px;margin-top:5px;margin-bottom:5px;margin-left:13px;' onclick='summaryPageAction(\"" + type + "\", \"" + state + "\");'>";
        } else {
                document.getElementById(type + "_button").innerHTML="";
        }
    } else {
        document.getElementById(type + "_button").innerHTML = "<input class='buttonDisabled' disabled='disabled' type='submit' name='" + type + "_" + state + "'" +
                "id='button_" + type + "' value='Turn Off' " + "title='Turn " + type + " Off' style='width:80px;margin-top:5px;margin-bottom:5px;margin-left:13px;'>";
        document.getElementById(type + "_ball").innerHTML = display_state;
    }
    return;
}
var actions = new Array();
var onoffActions = new Array();
var buttons = new Array();

if (enableHostSettings) {
    onoffActions.push({name:"Power&nbsp;State<br \/><span id='power_status'><\/span>",value:"<div id='Power_ball' style='display:inline'>&nbsp;<\/div>",nonamespan:true});
    buttons.push({name:"<div id='Power_button' style='min-height:29px;white-space:nowrap'>&nbsp;<\/div>", nonamespan:true});
}

if (enableILOMSettings & !ht_isDomainSP()) {
    onoffActions.push({name:"<div style='width:100px'>Locator&nbsp;Indicator<\/div>",value:"<div id='Locator_ball' style='display:inline'>&nbsp;<\/div>",nonamespan:true});
    buttons.push({name:"<div id='Locator_button' style='display:inline;white-space:nowrap;'>&nbsp;<\/div>", nonamespan:true});
}
if (enableHostSettings || (enableILOMSettings && !ht_isDomainSP())) {
    onoffActions.push({name:"&nbsp;", value:"&nbsp;"});
    buttons.push({name:"<div style='min-height:29px;'>&nbsp;<\/div>", nonamespan:true});
}





if (enableILOMSettings) {
    onoffActions.push({name:'System&nbsp;Firmware&nbsp;Update', value:""});
    buttons.push({name:"<input class='button' type='submit' " +
        "name='updateFW' id='Update' value='Update' title='Update System Firmware' " +
        "style='width:80px;margin-top:5px;margin-bottom:5px;margin-left:13px;' onclick='summaryPageAction(\"Firmware\", \"\");'>", nonamespan:true});
}



if (enableHostSettings) {

        {
            onoffActions.push({name:'Remote&nbsp;Console', value:""});
            buttons.push({name:"<input class='button' type='submit' " +
                "name='launchJRC' id='JRC' value='Launch' title='Launch Remote Console' " +
                "style='width:80px;margin-top:5px;margin-bottom:5px;margin-left:13px;' onclick='doLaunchJava(); return false;'>", nonamespan:true});
        }
}

if (onoffActions.length > 0) {
    actions.push({meta:{settable:'false'}, data:{actionCol:onoffActions, buttonCol:buttons}});
}
showAsTable({ary:actions,
             title:'Actions',
             stripe:'table',
             layoutTable:true,
             nvColumns:Array('actionCol', 'buttonCol'),
             useColNames:false
            });

if (enableHostSettings) {
    makeBall(sysinfoData.power_state, "Power");
}

if (enableILOMSettings && !ht_isDomainSP()) {
    makeBall(sysinfoData.locator_indicator, "Locator");
}
