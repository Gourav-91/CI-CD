    serverstatus.push({meta:{settable:'false'}, data:{row_index:'ord', items:ordSpan, status:ordHealth, details:ordDetails, inventory:ordInv}});
}
var statusHeading = null;
if (openProblemsViewable()) {
    var problemCount = DefaultInit(sysinfoData.open_problems_count, 0);
    var problemLink;
    var problemClick;
    if (problemCount >= 1) {
        problemClick = "return viewOpenProblems();";
        problemLink = "onClick='" + problemClick + "' title='View Open Problems' href='#'";
    }
    statusHeading = new Array({ name:'Overall Status', value:healthToIcon('System', 0, problemClick, 'tableActionLink') },
                              { name:'Total Problem Count', value:problemCount, link:problemLink });
}
var sampleHealth = "<span style='visibility:hidden'>" + healthToIcon() + "<\/span>";
var statusTableSummary = "The Status table displays the overall system health";
if (openProblemsViewable()) {
    statusTableSummary += " and total problem count";
}
statusTableSummary += " as well as the health, details, and inventory for each major subsystem.";
showAsTable({ary:serverstatus,
             title:'Status',
             nvColumns:Array('details', 'inventory'),
             stripe:'columns',
             col_meta:{
                 status:{nowrap:'nowrap'},
                 inventory:{namewidth:'1'},
                 details:{namewidth:'1'}},
             columnnames:{
                 items:'Subsystem',
                 status:'Status',
                 details:'Details',
                 inventory:'Inventory'},
             heading:statusHeading,
             sampleHealth:sampleHealth,
             width:'100%',
             summary:statusTableSummary
            });
</script>
