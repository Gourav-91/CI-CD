index = sysinfoIndex.Networking;
if (typeof(index) == "number") {
    var networkingOnClick = "ht_selectTreeNode(\"System_Information\", \"Networking\"); return false;";
    var networkingDetails = new Array(emptyPair);
    var networkingInv = new Array();
    if (typeof(sysinfo[index].data.installed_ethernet_switches) != "undefined") {
       networkingInv.push({name:'Switches:',
            value:FormatInstalledMax(sysinfo[index].data.installed_ethernet_switches)});
    }
    if (typeof(sysinfo[index].data.installed_eth_nics) != "undefined") {
        networkingInv.push({name:'Ethernet NICs:',
            value:FormatInstalledMax(sysinfo[index].data.installed_eth_nics)});
    }
