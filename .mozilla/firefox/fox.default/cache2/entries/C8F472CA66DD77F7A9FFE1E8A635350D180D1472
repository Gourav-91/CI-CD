    var procNum = FormatInstalledMax(sysinfo[index].data.installed_cpus, sysinfo[index].data.max_cpus);
    var procDetails = new Array({name:'Processor Architecture:', value:sysinfo[index].data.architecture},
        {name:'Processor Summary:', value:sysinfo[index].data.summary_description});
    var procInv = new Array({name:'Processors:', value:procNum});
    var procSpan = "<a onClick='" + procOnClick + "' href='#' title='Processors'>Processors<\/a>";
    var procHealth = healthToIcon("Processors", index, procOnClick);
    serverstatus.push({meta:{settable:'false'}, data:{row_index:'processors', items:procSpan, status:procHealth, details:procDetails, inventory:procInv}});
}
index = sysinfoIndex.Memory;
if (typeof(index) == "number") {
    var memOnClick = "ht_selectTreeNode(\"System_Information\", \"Memory\"); return false;";
    var memDetails = new Array({name:'Installed RAM Size:', value:DefaultInnerHtmlNA(sysinfo[index].data.installed_memory)});
    var memInvDetails = FormatInstalledMax(sysinfo[index].data.installed_dimms, sysinfo[index].data.max_dimms);
    var memInv = new Array({name:'DIMMs:', value:memInvDetails}, emptyPair);
    var memSpan = "<a onClick='" + memOnClick + "' href='#' title='Memory'>Memory<\/a>";
    var memHealth = healthToIcon("Memory", index, memOnClick);
    serverstatus.push({meta:{settable:'false'}, data:{row_index:'memory', items:memSpan, status:memHealth, details:memDetails, inventory:memInv}});
}
index = sysinfoIndex.Power;
if (typeof(index) == "number") {
    var powOnClick = "ht_selectTreeNode(\"System_Information\", \"Power\"); return false;";
    var powPermittedVal = FormatPowerValueNA(sysinfo[index].data.max_permitted_power);
    var powActualVal = FormatPowerValueNA(sysinfo[index].data.actual_power_consumption);
    var powInvDetails = FormatInstalledMax(sysinfo[index].data.installed_power_supplies, sysinfo[index].data.max_power_supplies);
    var powDetails = new Array({name:'Permitted Power Consumption:', value:powPermittedVal},
        {name:'Actual Power Consumption:', value:powActualVal});
    var powInv = new Array({name:'PSUs:', value:powInvDetails});
    var powSpan = "<a onClick='" + powOnClick + "' href='#' title='Power'>Power<\/a>";
    var powHealth = healthToIcon("Power", index, powOnClick);
    serverstatus.push({meta:{settable:'false'}, data:{row_index:'power', items:powSpan, status:powHealth, details:powDetails, inventory:powInv}});
}
index = sysinfoIndex.Cooling;
if (typeof(index) == "number") {
    var coolOnClick = "ht_selectTreeNode(\"System_Information\", \"Cooling\"); return false;";
    var inletTemp = FormatTemp(sysinfo[index].data.inlet_temp);
    var exhaustTemp = FormatTemp(sysinfo[index].data.exhaust_temp);
    var chassisFanMod = FormatInstalledMax(sysinfo[index].data.installed_chassis_fans, sysinfo[index].data.max_chassis_fans);
    var psuFanMod = FormatInstalledMax(sysinfo[index].data.installed_power_supply_fans, sysinfo[index].data.max_power_supply_fans);
    var coolDetails = new Array({name:'Inlet Air Temperature:', value:inletTemp}, {name:'Exhaust Air Temperature:', value:exhaustTemp});
    var coolInv = new Array({name:'Chassis Fans:', value:chassisFanMod}, {name:'PSU Fans:', value:psuFanMod});
    var coolSpan = "<a onClick='" + coolOnClick + "' href='#' title='Cooling'>Cooling<\/a>";
    var coolHealth = healthToIcon("Cooling", index, coolOnClick);
    serverstatus.push({meta:{settable:'false'}, data:{row_index:'cooling', items:coolSpan,
        status:coolHealth, details:coolDetails, inventory:coolInv}});
}
index = sysinfoIndex.Storage;
if (typeof(index) == "number") {
