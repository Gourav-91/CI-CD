index = sysinfoIndex.Other_Removable_Devices;
if (typeof(index) == "number") {
    var ordOnClick = "ht_selectTreeNode(\"System_Information\", \"Other_Removable_Devices\"); return false;";
    var ordDetails = new Array(emptyPair, emptyPair);
    var ordInv = new Array(emptyPair, emptyPair);
    var ordSpan = "<a onClick='" + ordOnClick + "' href='#' title='Other Reovable Devices'>Other Removable Devices<\/a>";
