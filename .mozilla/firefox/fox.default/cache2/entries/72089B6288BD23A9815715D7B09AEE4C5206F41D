row_action_names = new Array();
row_action_names2 = new Array();
Array.prototype.matchData = function(field, value) {
    var newArray = new Array();
    var regExp = new RegExp(value);
    for (var i=0; i<this.length; ++i) {
        var subsetData = this[i]['data'][field];
        if (subsetData && regExp.test(subsetData)) {
            newArray.push(this[i]);
        }
    }
    return newArray;
}
Array.prototype.skipData = function(field, value) {
    var newArray = new Array();
    var regExp = new RegExp(value);
    for (var i=0; i<this.length; ++i) {
        var subsetData = this[i]['data'][field];
        if (!subsetData || !regExp.test(subsetData)) {
            newArray.push(this[i]);
        }
    }
    return newArray;
}
Array.prototype.swap = function(a, b) {
    var tmp = this[a];
    this[a] = this[b];
    this[b] = tmp;
    return this;
}
if (!Array.prototype.indexOf) Array.prototype.indexOf = function(item, i) {
  i || (i = 0);
  var length = this.length;
  if (i < 0) i = length + i;
  for (; i < length; i++)
    if (this[i] == item) return i;
  return -1;
};
renameAssocArrayKey = function(obj, src, dst) {
    var v = obj[src];
    delete(obj[src]);
    obj[dst] = v;
}
String.prototype.trim = function(chr) {
    chr = DefaultInit(chr, " ");
    if (chr.length != 1) {
        alert("Developer Error: Can only use a single character with trim(). You used:'" + chr + "'");
        return this;
    }
    var r = new RegExp("^[" + chr + "]+|[" + chr + "]+$", "g");
    return this.replace(r, "");
}
String.prototype.collapse = function(chr) {
    chr = DefaultInit(chr, " ");
    if (chr.length != 1) {
        alert("Developer Error: Can only use a single character with collapse(). You used:'" + chr + "'");
        return this;
    }
    var r = new RegExp("[" + chr + "]{2,}", "g");
    return this.replace(r, chr);
}
String.prototype.initcap = function() {
    return this.charAt(0).toUpperCase() + this.substr(1);
}
String.prototype.safeQuote = function(q) {
    var from, to;
    if (q == "'") {
        from = "'"; to = '"';
    } else if (q == '"') {
        from = '"'; to = "'";
    }
    if (this.contains(from) && this.contains(to)) {
        alert("Developer Error: String contains both single and double quotes. Continuing...");
        return this;
    }
    var r = new RegExp(from, "g");
    return this.replace(r, to);
}
String.prototype.capitalize = function() {
    return this.replace(/\w+/g, function(a) {
        return a.charAt(0).toUpperCase() + a.substr(1);
    });
}
String.prototype.replace_word = function(find, replace) {
    return this.replace(new RegExp("\\b" + find + "\\b", "g"), replace);
}
String.prototype.strcmp = function(str) {
    return (this == str);
}
String.prototype.strcasecmp = function(str) {
    return (this.toLowerCase() == str.toLowerCase());
}
String.prototype.make_html_id = function() {
    return this.replace(/[^A-Za-z0-9:._-]/g, "_");
}
String.prototype.decodeJS = function() {
    return this.replace(/&lt\;/g, "<").replace(/&gt\;/g, ">");
}
String.prototype.toBoolean = function() {
    if (this == "" ||
        this.toLowerCase() == "false" ||
        this == 0) {
        return false;
    }
    return true;
}
String.prototype.contains = function(str) {
    return this.indexOf(str) >= 0;
}
String.prototype.beginsWith = function(str) {
    return this.indexOf(str) == 0;
}
String.prototype.endsWith = function(str) {
    var pos = this.length - str.length;
    return pos >= 0 && this.lastIndexOf(str) == pos;
}
String.prototype.appendNewline = function() {
    if (!this.endsWith("\n")) {
        return this + "\n";
    }
    return this;
}
String.prototype.commaSpace = function() {
    return this.replace(/,([^ ])/g, ", $1");
}
String.prototype.query_append = function(name, value) {
    if (typeof(name) != "undefined") {
        var s = (this == "") ? '?' : '&';
        s += name;
        if (typeof(value) != "undefined") {
            s += "=" + value
        };
        return this.concat(s);
    }
    return this;
}
String.prototype.linkify = function() {
    //   Document Reference:http://oracle.com/blah
    //   Some text (some text. http://oracle.com/blah)
    //   <b>http://oracle.com/blah</b>
    //   Some text http://oracle.com/blah some text
    //   A sentence http://oracle.com/blah.
    return this.replace(/http[s]?:\/\/[^)< ]+[^.)< ]/g, "<a href='$&' target='_blank'>$&<\/a>");
}
String.prototype.escapeQuotes = function() {
    return this.replace(/"/g, '\\"').replace(/'/g, "\\'");
}
function ascii(c) {
    return c.charCodeAt(0);
}
function dec2hex(i) {
    return i.toString(16);
}
String.prototype.encode = function(c) {
    var r = new RegExp(c, "g");
    return this.replace(r, "%" + dec2hex(ascii(c)));
}
function userPwEscapeEncode(userpwstr) {
    return userpwstr.encode("%").encode(":").encode("@").encode("/");
}
function getKeyCode(e) {
    if (window.event) {
        return window.event.keyCode;
    } else if (e) {
        return e.which;
    }
}
function getKeyChar(e) {
    return String.fromCharCode(getKeyCode(e));
}
function isNumber(str) {
    return str.trim().length > 0 && !isNaN(str);
}
function string2int(str) {
    if (isNumber(str))
        return parseInt(str, 10);
    return 0;
}
function int2string(i) {
    return i + "";
}
function hasItems(ary) {
   return (ary && ary.length > 0);
}
function hideElement(obj) {
    var elem = getElement(obj);
    if (elem) elem.style.display = "none";
}
function showElement(obj) {
    var elem = getElement(obj);
    if (elem) elem.style.display = "";
}
function invisibleElement(obj) {
    var elem = getElement(obj);
    if (elem) elem.style.visibility = "hidden";
}
function visibleElement(obj) {
    var elem = getElement(obj);
    if (elem) elem.style.visibility = "visible";
}
function isElementHidden(obj) {
    var elem = getElement(obj);
    return (elem.style.display == "none" || elem.style.visibility == "hidden");
}
function isElementVisible(obj) {
    return !isElementHidden(obj);
}
function LZ(n) { return (n!=null && n<10 && n>=0 ? "0" : "") + Number(n) }
function LZZ(n) { return (n!=null && n<100 && n>=0 ? "0" + LZ(n) : "" + Number(n)) }
function DefaultInit(a, b) {
    return (a ? a : b);
}
function DefaultInnerHtml(a) {
    return DefaultInit(EscapeInnerHtml(a), "<i>(none)<\/i>");
}
function EscapeInnerHtml(a) {
    if (a != undefined) { return a.replace(/</g,'&lt;'); }
    return a;
}
function DefaultInnerHtmlNA(val) {
    return DefaultInit(EscapeInnerHtml(val), "Not Available");
}
function FormatTemp(val) {
    if (val == "" || val == undefined) {
        return DefaultInnerHtmlNA(val);
    } else {
        var tempVal = val.replace(/[Dd]egree[s]?[ ]*C/, "&deg;C");
        return tempVal;
    }
}
function FormatInstalledMax(installed, max)
{
    installed = DefaultInnerHtmlNA(installed);
    if (installed == max && !isNumber(installed)) {
        return (installed);
    }
    if (typeof(max) == "undefined") {
        return (installed + '&nbsp;<span class="subscriptLabel">(Installed)<\/span>');
    }
    return (installed + '&nbsp;/&nbsp;' + max + '&nbsp;<span class="subscriptLabel">(Installed&nbsp;/&nbsp;Maximum)<\/span>');
}
function alertMessage(message, setfocus) {
    if (message != "") {
        alert(message);
        if (setfocus) getElement(setfocus).focus();
        return false;
    }
    return true;
}
function ht_set_field_value(field, value) {
    var elem = getElement(field);
    if (elem) {
        if (elem.type == "select-one") {
            for (var i=0; i<elem.length; i++) {
                if (value.strcasecmp(elem.options[i].value)) {
                    elem.options[i].selected = true;
                    return;
                }
            }
        } else {
            elem.value = value;
        }
    }
}
function ht_set_field_text(field, value) {
    var elem = getElement(field);
    if (elem) {
        if (elem.type == "select-one") {
            for (var i=0; i<elem.length; i++) {
                if (value.strcasecmp(elem.options[i].text)) {
                    elem.options[i].selected = true;
                    return;
                }
            }
        } else {
            elem.text = value;
        }
    }
}
function FormatPowerValueInternal(val, naString) {
    naString = DefaultInit(naString, "Not available");
    var sVal = val + "";
    var iVal = parseInt(val);
    if (val == undefined || sVal == "" || sVal == "NaN") {
        return naString;
    }
    if (isNaN(iVal)) {
        return sVal;
    }
    var suffix = "%";
    if (sVal.indexOf("%") == -1) {
        suffix = " watt";
        if (iVal != 1) {
            suffix += "s";
        }
    }
    return iVal + suffix;
}
function FormatPowerValueNA(val) {
    return FormatPowerValueInternal(val, "Not available");
}
function FormatPowerValueNone(val) {
    return FormatPowerValueInternal(val, "(none)");
}
function stashStore(label, data) {
    if (typeof(__HT_STASH) == 'undefined') {
        __HT_STASH = new Object();
    }
    __HT_STASH[label] = data;
}
function stashRetrieve(label) {
    var theStash = _stashReference();
    if (theStash) {
        var tmp = theStash[label];
        if (typeof(theStash[label]) != "undefined") {
            delete theStash[label];
        }
        return tmp;
    }
    return null;
}
function stashDump(label) {
    var theStash = _stashReference();
    for (var i in theStash) {
        if (label && i != label) continue;
        var m = ""
        if (typeof(theStash[i]) == "object") {
            for (var j in theStash[i]) {
                if (typeof(theStash[i][j]) != "function") {
                    m += "stash" + "[" + i + "]" + "[" + j + "]";
                    m += " = '" + theStash[i][j] + "'" + "\n";
                }
            }
        } else {
            m += "stash" + "[" + i + "]";
            m += " = '" + theStash[i] + "'" + "\n";
        }
        alert(m);
    }
}
function _stashReference() {
    if (top && top.opener && top.opener.__HT_STASH) {
        return top.opener.__HT_STASH;
    }
    if (typeof(__HT_STASH) != 'undefined') {
        return __HT_STASH;
    }
    return null;
}
function ht_getParentTableElement(elem) {
    var pid = elem.id;
    pid = String(pid).replace(/_Btn[TB]?[0-9]+$/,"");
    pid = String(pid).replace(/_Sel[0-9]+$/,"");
    return document.getElementById(pid);
}
function ht_getParentFormElement(elem) {
    var pid = elem.id;
    pid = String(pid).replace(/_Btn[TB]?([0-9]+)$/,"_Form");
    pid = String(pid).replace(/_Sel([0-9]+)$/,"_Form");
    pid = String(pid).replace(/_Dropdown$/,"_Form");
    pid = String(pid).replace(/_Dropdown2$/,"_Form");
    return document.getElementById(pid);
}
function ht_getSelectedObj(form, ary) {
    var selectedRowValues = ht_getSelectedRowNumbers(form);
    var row_index;
    var obj_name;
    var obj_path;
    if (selectedRowValues[0].indexOf(':') != -1) {
        var vals = selectedRowValues[0].split(':');
        row_index = vals[0];
        obj_name = ary[row_index].data.crows[vals[1]].data.obj_name;
        obj_path = ary[row_index].data.row_index + "/" + obj_name.replace(/ /g, '_');
    } else {
        row_index = selectedRowValues[0];
        obj_name = ary[row_index].data.obj_name;
        obj_path = ary[row_index].data.row_index;
    }
    return {row:row_index, name:obj_name, path:obj_path};
}
function ht_getSelectedItems(form, ary) {
    var selectedItems = new Array();
    var selectedRows = ht_getSelectedRows(form, ary);
    for (var elem=0; elem < selectedRows.length; elem++) {
        selectedItems[selectedItems.length] = selectedRows[elem].data.row_index;
    }
    return selectedItems;
}
function ht_getSelectedRows(form, ary) {
    var selectedRows = new Array();
    var row_numbers = ht_getSelectedRowNumbers(form);
    for (var elem = 0; elem < row_numbers.length; elem++) {
        selectedRows[selectedRows.length] = ary[row_numbers[elem]];
    }
    return selectedRows;
}
function ht_getSelectedRowNumbers(form) {
    var selectedRowNumbers = new Array();
    for (var elem = 0; elem < form.elements.length; elem++) {
        if (form.elements[elem].checked)
            selectedRowNumbers[selectedRowNumbers.length] = form.elements[elem].value;
    }
    return selectedRowNumbers;
}
function ht_disableAllDropdownOptions(args) {
    if (document.getElementById(args.dropdown_id) == null ||
            typeof(document.getElementById(args.dropdown_id)) == 'undefined')
        return;
    var all_options = document.getElementById(args.dropdown_id).options;
    for (var i = 1; i < all_options.length; i++) {
        ht_set_dropdown_option_state({ obj:all_options[i], state:'disabled' });
    }
}
function ht_enableAllDropdownOptions(args) {
    if (document.getElementById(args.dropdown_id) == null ||
            typeof(document.getElementById(args.dropdown_id)) == 'undefined')
        return;
    var all_options = document.getElementById(args.dropdown_id).options;
    for (var i = 1; i < all_options.length; i++) {
        ht_set_dropdown_option_state({ obj:all_options[i], state:'enabled' });
    }
}
function ht_set_dropdown_option_state(args) {
    var state = args.state;
    var opt = args.obj;
    if (state == "enabled") {
        opt.className = opt.className.replace(/Dis$/, "");
        opt.disabled = false;
    } else if (state == "disabled") {
        if (opt.selected) {
            opt.parentNode.selectedIndex = 0;
        }
        if (!opt.className.endsWith("Dis")) {
            opt.className += "Dis";
        }
        opt.disabled = true;
    } else {
        return;
    }
}
function get_dropdown_option_selected(arg) {
    var select, option;
    select = getElement(arg);
    if (!select.type || select.type != "select-one") {
        option = arg;
    }
   if (typeof(select) != "undefined" && select.options) {
       option = select.options[select.selectedIndex];
   }
   return typeof(option) != "undefined" ? option : null;
}
function ht_is_dropdown_option_enabled(arg) {
    var option = get_dropdown_option_selected(arg);
   if (option != null && option.disabled != undefined) {
       return !(new Boolean(option.disabled).valueOf());
   }
   return false;
}
function ht_handleButtonsEmptyTable(args) {
    for (var i = 0; i < args.buttons.length; i++) {
        if (args.buttons[i].whenEmpty == "disabled") {
            var button_id = args.table_id + "_BtnT" + i;
            if (getElement(button_id)) {
                setButtonDisable(button_id);
            }
        }
    }
}
function ht_enableDisableButtons(args) {
    var buttons = args.enabled_buttons;
    if (buttons == undefined) {
        buttons = "ALL";
    }
    var buttonList;
    if (buttons && buttons != "ALL") {
        buttonList = String(buttons).split(",");
        for (var j = 0; j < buttonList.length; j++) {
            buttonList[j] = buttonList[j].trim();
        }
    }
    var buttonId, thisButton;
    var i = 0;
    while (1) {
        buttonId = args.table_id + "_BtnT" + i;
        thisButton = getElement(buttonId);
        if (!thisButton) {
            return;
        }
        if (buttons == "") {
            setButtonDisable(buttonId);
            buttonId = String(buttonId).replace(/_BtnT/, "_BtnB");
            setButtonDisable(buttonId);
        } else if (buttons == "ALL") {
            setButtonEnable(buttonId);
            buttonId = String(buttonId).replace(/_BtnT/, "_BtnB");
            setButtonEnable(buttonId);
        } else {
            var found = false;
            for (var j = 0; j < buttonList.length; j++) {
                if (buttonList[j] == thisButton.value || thisButton.innerHTML == buttonList[j]) {
                    setButtonEnable(buttonId);
                    buttonId = String(buttonId).replace(/_BtnT/, "_BtnB");
                    setButtonEnable(buttonId);
                    found = true;
                    break;
                }
            }
            if (!found) {
                setButtonDisable(buttonId);
                buttonId = String(buttonId).replace(/_BtnT/, "_BtnB");
                setButtonDisable(buttonId);
            }
        }
        i++;
    }
}
function ht_processTableClick(args) {
    var selectedRowID;
    if (args.crow) {
        selectedRowID = args.crow + "_SelRow";
    } else {
        selectedRowID = args.table_id + "_SelRow" + args.row;
    }
    var elem = getElement(selectedRowID);
    var stripeRows = DefaultInit(args.stripeRows, false);
    var form = getElement(args.table_id + "_Form");
    var idx = 0;
    var row;
    for (var i = 0; i < form.elements.length; i++) {
        if (form.elements[i].id.indexOf("_SelRow") != -1) {
            if (form.elements[i].id == selectedRowID) {
                form.elements[i].checked = true;
            }
            row = getElement(String(form.elements[i].id).replace(/_SelRow/, "_Row"));
            if (form.elements[i].id.indexOf(args.table_id + "_SelRow") != -1) {
                if (stripeRows) {
                    row.className = getStripeClass(idx);
                } else {
                    row.className = "";
                }
                idx++;
            } else {
                row.className = "rowClassSubRow";
            }
            if (args.row_actions || args.enabled_buttons) {
                row.className += " tableRowAction";
            }
            row.setAttribute("aria-selected", "false");
            row.setAttribute("aria-label", "unselected");
        }
    }
    if (args.crow) {
        row = getElement(args.crow + "_Row");
    } else {
        row = getElement(args.table_id + "_Row" + args.row);
    }
    row.className = "tableRowSelected";
    row.setAttribute("aria-selected", "true");
    row.setAttribute("aria-label", "selected");
    ht_enableDisableButtons({table_id:args.table_id, enabled_buttons:args.enabled_buttons});
    ht_disableAllDropdownOptions({dropdown_id:args.table_id + "_Dropdown"});
    ht_disableAllDropdownOptions({dropdown_id:args.table_id + "_Dropdown2"});
    if (args.row_actions == undefined) {
        return;
    }
    if (typeof(args.row_actions) == "string") {
        row_actions = args.row_actions.split(",");
    } else {
        row_actions = args.row_actions;
    }
    for (var i = 0; i < row_actions.length; i++) {
        var selected_option_id = row_action_names[args.table_id + "_" + row_actions[i]];
        if (selected_option_id) {
            var selected_option = getElement(selected_option_id);
            if (selected_option.disabled) {
                ht_set_dropdown_option_state({obj:selected_option, state:'enabled'});
            }
        }
    }
    for (var i = 0; i < row_actions.length; i++) {
        var selected_option_id = row_action_names2[args.table_id + "_" + row_actions[i]];
        if (selected_option_id) {
            var selected_option = getElement(selected_option_id);
            if (selected_option.disabled) {
                ht_set_dropdown_option_state({obj:selected_option, state:'enabled'});
            }
        }
    }
}
function ht_deselectAll(args) {
    var form = getElement(args.table_id + "_Form");
    var stripeRows = DefaultInit(args.stripeRows, false);
    var idx = 0;
    for (var i = 0; i < form.elements.length; i++) {
        if (form.elements[i].id.indexOf("_SelRow") != -1) {
            var stripeClass = "";
            if (stripeRows) {
                stripeClass = getStripeClass(idx);
            }
            idx++;
            form.elements[i].checked = false;
            row = getElement(String(form.elements[i].id).replace(/_SelRow/, "_Row"));
            row.className = stripeClass;
        }
    }
    ht_disableAllDropdownOptions({dropdown_id:args.table_id + "_Dropdown"});
    ht_disableAllDropdownOptions({dropdown_id:args.table_id + "_Dropdown2"});
}
function ht_resetDropdown(id) {
    getElement(id).selected=true;
}
function showAsDropdown(args) {
    var i;
    var selID = (args.id != null) ? args.id : "";
    var selOnChange = (args.onChange != null) ? args.onChange : "";
    var selState = "class='menuStd'";
    if (args.state != null) {
        var lower_state = args.state.toLowerCase();
        if (lower_state == 'disabledjump' || lower_state == "disabledaction") {
            selState = "disabled";
        } else if (lower_state == 'jump'  || lower_state == "action") {
            selState = "";
        } else if (lower_state == 'disabled') {
            selState = "disabled";
        } else if (lower_state == 'filtered') {
            selState = "";
        } else if (lower_state == 'hidden') {
            selState = "style='display:none'";
        }
    }
    if (args.width) {
        if (selState == "style='display:none'") {
            selState = "style='display:none;width:" + args.width + "'";
        } else {
            selState += " style='width:" + args.width + "'";
        }
    }
    if (args.className) {
        selState += " class='" + args.className+ "'"
    }
    document.write("<select name='" + selID + "' id='" + selID + "'");
    document.write(" " + selState);
    if ((args.multi != null) && (args.multi == "true")) {
        document.write(" multiple='multiple'");
        if (args.multiSize != null) {
            document.write(" size='");
            if (args.multiSize == "all") {
                document.write(args.ary.length);
            } else {
                document.write(args.multiSize);
            }
            document.write("'");
        }
    }
    document.write(" onfocus='this.prevIndex=this.selectedIndex;'");
    document.write(" onchange='if (ht_is_dropdown_option_enabled(this))");
    document.write(" {this.prevIndex=this.selectedIndex;" + selOnChange + "}");
    document.write(" else {this.selectedIndex=this.prevIndex;}'");
    if (args.ariaDescribedby) {
        document.write(" aria-describedby='" + args.ariaDescribedby + "'");
    }
    document.write(">\n");
    for (var i=0; i<args.ary.length; i++) {
        var thisOpt = args.ary[i];
        if (thisOpt.value != null) {
            var optValue = (thisOpt.value != "'") ? thisOpt.value : "&#39;";
        } else {
            var optValue = "";
        }
        if (thisOpt.text != null) {
            var optText = (thisOpt.text != "<") ? thisOpt.text : "&lt;";
        } else {
            var optText = optValue;
        }
        var optState = "";
        if (thisOpt.state != null) {
            var lower_state = thisOpt.state.toLowerCase();
            if (lower_state == 'disabledjump' || lower_state == "disabledaction") {
                optState = "disabled";
            } else if (lower_state == 'selectedjump' || lower_state == "selectedaction") {
                optState = "selected='selected'";
            } else if (lower_state == 'disabled') {
                optState = "disabled";
            } else if (lower_state == 'selected') {
                optState = "selected='selected' class='optText'";
            }
        }
        document.write("<option");
        if ((args.opt_id == null) || (args.opt_id == "true")) {
            var optID = (thisOpt.id != null) ? thisOpt.id : selID + "_Opt" + optValue.make_html_id();
            document.write(" id='" + optID + "'");
        }
        document.write(" value='" + optValue + "'");
        if (optState) {
            document.write(" " + optState);
        }
        if (thisOpt.ttip) {
            document.write(" title='" + thisOpt.ttip + "'");
        }
        document.write(">" + optText + "</option>\n");
    }
    document.write("</select>\n");
}
function setDropdownEnable(args) {
    if (args.select && args.option) {
        var option = document.getElementById(args.select + "_Opt" + args.option);
        if (option) ht_set_dropdown_option_state({state:'enabled', obj:option});
    } else if (args.option) {
        var option = getElement(args.option);
        if (option) ht_set_dropdown_option_state({state:'enabled', obj:option});
    } else if (args.select) {
        ht_enableAllDropdownOptions({dropdown_id:args.select.id});
    } else {
        var dropdownObj = getElement(args);
        dropdownObj.disabled = false;
        if (dropdownObj.className.endsWith("Dis")) {
            dropdownObj.className = dropdownObj.className.replace(/Dis$/, "");
        }
    }
}
function setDropdownDisable(args) {
    if (args.select && args.option) {
        var option = document.getElementById(args.select + "_Opt" + args.option);
        if (option) ht_set_dropdown_option_state({state:'disabled', obj:option});
    } else if (args.option) {
        var option = getElement(args.option);
        if (option) ht_set_dropdown_option_state({state:'disabled', obj:option});
    } else if (args.select) {
        ht_disableAllDropdownOptions({dropdown_id:args.select.id});
    } else {
        var dropdownObj = getElement(args);
        dropdownObj.disabled = true;
        if (!dropdownObj.className.endsWith("Dis")) {
            dropdownObj.className += "Dis";
        }
    }
}
function enableDropdown(field, enabled) {
    if (enabled) {
        setDropdownEnable(field);
    } else {
        setDropdownDisable(field);
    }
}
function getElement(field) {
    if (typeof(field) == "string") {
        return document.getElementById(field);
    }
    return field;
}
function getElementValue(field) {
    return getElement(field).value;
}
function getElementInnerValue(field) {
    return getElement(field).innerHTML;
}
function setElementValue(field, value) {
    if (typeof(value) == "string") {
        getElement(field).value = value.decodeJS();
    } else {
        getElement(field).value = value;
    }
}
function setElementInnerValue(field, value) {
   getElement(field).innerHTML = DefaultInnerHtml(value);
}
function setButtonDisable(btn) {
    var buttonObj = getElement(btn);
    if (buttonObj) {
        buttonObj.disabled = true;
        if (!buttonObj.className.endsWith("Disabled")) {
            buttonObj.className += "Disabled";
        }
    }
}
function setButtonEnable(btn) {
    var buttonObj = getElement(btn);
    if (buttonObj) {
        buttonObj.disabled = false;
        buttonObj.className = buttonObj.className.replace(/Disabled$/,"");
    }
}
function enableButton(btn, enabled) {
    if (enabled) {
        setButtonEnable(btn);
    } else {
        setButtonDisable(btn);
    }
}
function setTextFieldDisable(field) {
    var fieldObj = getElement(field);
    fieldObj.disabled = true;
    if (!fieldObj.className.endsWith("Dis")) {
        fieldObj.className += "Dis";
    }
}
function setTextFieldEnable(field) {
    var fieldObj = getElement(field);
    fieldObj.disabled = false;
    if (fieldObj.className.endsWith("Dis")) {
        fieldObj.className = fieldObj.className.replace(/Dis$/, "");
    }
}
function enableTextField(field, enabled) {
    if (enabled) {
        setTextFieldEnable(field);
    } else {
        setTextFieldDisable(field);
    }
}
function setCheckboxDisable(field) {
    var fieldObj = getElement(field);
    fieldObj.disabled = true;
    fieldObj.className = fieldObj.className.substr(0,2) + "Dis";
}
function setCheckboxEnable(field) {
    var fieldObj = getElement(field);
    fieldObj.disabled = false;
    fieldObj.className = fieldObj.className.substr(0,2);
}
function enableCheckbox(field, enabled) {
    if (enabled) {
        setCheckboxEnable(field);
    } else {
        setCheckboxDisable(field);
    }
}
function textWrap(longstr, maxwide, lineDelim) {
    var delim = (typeof(lineDelim)=="string" ? lineDelim : "<br />");
    var lines = Math.ceil(longstr.length / maxwide);
    var wrappedstr = "";
    var j = 0;
    for (var i=0; i<lines; i++) {
        if (i>0) wrappedstr += delim;
        wrappedstr += longstr.substr(j, maxwide);
        j += maxwide;
    }
    return wrappedstr;
}
function csvSetSelectOptions(arg, csv) {
    var ary = getElement(arg);
    var vary = csv.split(",");
    for (var i=0; i<ary.options.length; i++) {
        for (var j=0; j<vary.length; j++) {
            if (ary.options[i].value.trim() == vary[j].trim()) {
                ary.options[i].selected = true;
            }
        }
    }
}
function getSelectOptionCount(arg) {
    var opts = getElement(arg).options;
    var count = 0;
    for (var i=0; i<opts.length; i++) {
        if (opts[i].selected == true) {
            count++;
        }
    }
    return count;
}
function selectOption2csv(arg) {
    var csv = '';
    var ary = getElement(arg);
    for (var i=0; i<ary.options.length; i++) {
        if (ary.options[i].selected) {
            var txt = ary.options[i].value.trim();
            csv += txt + ", ";
        }
    }
    csv = csv.substring(0, csv.length - 2);
    return csv;
}
function csv2tvary(csv, empty_txt, empty_val) {
    var val = '';
    var txt = '';
    var ary = csv.split(',');
    var tvary = new Array();
    if (!empty_txt) { empty_txt = 'None'; }
    if (!empty_val) { empty_val = ''; }
    for (var i=0; i<ary.length; i++) {
        txt = val = ary[i].trim();
        if (val == '' || val == '\"\"' || val == '\'\'') {
            txt = empty_txt;
            val = empty_val;
        } else {
            txt = val.capitalize();
        }
        tvary.push({text:txt, value:val});
    }
    return tvary;
}
function ary2tvary(ary) {
    var tvary = new Array();
    var txt = '';
    var val = '';
    for (var i=0; i<ary.length; i++) {
        txt = val = ary[i].trim();
        tvary.push({text:txt, value:val});
    }
    return tvary;
}
function showCsvAsDropdown(args) {
    var csv = args.csv;
    if (args.empty_txt) {
        var empty_txt = args.empty_txt;
        delete args.empty_txt;
    }
    if (args.empty_val) {
        var empty_val = args.empty_val;
        delete args.empty_val;
    }
    var tvary = csv2tvary(csv, empty_txt, empty_val);
    for (var i=0; i<tvary.length; i++) {
        if (args.sel && tvary[i].value == args.sel) {
            tvary[i].state = 'selected';
        }
    }
    if (args.sel) { delete args.sel; }
    delete args.csv;
    args.ary = tvary;
    showAsDropdown(args);
}
function ht_togglePane(table_id, divid) {
    var pane = {prefs:document.getElementById(table_id + "prefs"),
                custfltr:document.getElementById(table_id + "custfltr")};
    for (i in pane) {
        if (pane[i]) { hideElement(pane[i]); }
        hideElement(table_id + "PrefsRow");
    }
    if (divid != "empty") {
        showElement(table_id + "PrefsRow");
        pane[divid].style.display = "block";
    }
}
function ht_tblKey(e, enterAction, acceptedChars) {
    var key;
    var keychar;
    key = getKeyCode(e);
    if (!key) return true;
    switch (key) {
        case null:
        case 0:
        case 8:
        case 9:
        case 27:
            return true;
            break;
        case 13:
            eval(enterAction);
            return false;
            break;
        default:
            ;
    }
    if (acceptedChars && acceptedChars != '') {
        keychar = String.fromCharCode(key);
        if (acceptedChars.indexOf(keychar) > -1) { return true; }
        else { return false; }
    }
    return true;
}
function ht_nthCRow(that, n) {
    return getElement(that.id + "_sr" + n + "_Row");
}
function ht_toggleCRows(that, state) {
    var elem = ht_nthCRow(that, 0);
    if (!elem) {
        return;
    }
    if (state == undefined) {
        if (elem.style.display == "none") {
            state = "";
        } else {
            state = "none";
        }
    }
    for (var i=0;
        elem && elem.style;
        elem = ht_nthCRow(that, i), i++) {
        elem.style.display = state;
    }
    elem = getElement(that.id + "_icon");
    if (state == "none") {
        that.title = "Click to Expand Group";
        elem.src = elem.src.replace("collapse", "expand");
        elem.alt = "Expand";
        if (elem.style.filter) {
            elem.style.filter = elem.style.filter.replace("collapse", "expand");
        }
    } else {
        that.title = "Click to Collapse Group";
        elem.src = elem.src.replace("expand", "collapse");
        elem.alt = "Collapse";
        if (elem.style.filter) {
            elem.style.filter = elem.style.filter.replace("expand", "collapse");
        }
    }
}
function ht_expandCRows(that) {
    ht_toggleCRows(that, "");
}
function ht_collapseCRows(that) {
    ht_toggleCRows(that, "none");
}
function getStripeClass(i) {
    if (i % 2 == 0) {
        return "tableRowEven";
    }
    return "tableRowOdd";
}
function getCellShade(i) {
    if (i % 2 == 0) {
        return "cellShadeDark";
    }
    return "cellShadeLight";
}
function writeTableCtrlRow(numColumns)
{
    document.write("  <tr class='tableCtrlRow'>\n");
    document.write("    <td colspan='" + numColumns + "'>\n");
}
function writeTableButtons(args)
{
    var last_was_aed = false;
    var suffix, styleStr;
    if (args.set == "bottom") {
        suffix = "_BtnB";
    } else {
        suffix = "_BtnT";
    }
    for (var button_i=0; button_i <args.buttons.length; button_i++) {
        if (args.buttons[button_i].value == "Add") {
            document.write("<button class='addAction' id='" + args.table_id +
                           suffix + button_i + "' onClick='" + args.buttons[button_i].onClick +
                           "'>Add</button>");
            last_was_aed = true;
        } else if (args.buttons[button_i].value == "Edit") {
            document.write("<button class='editAction' id='" + args.table_id +
                           suffix + button_i + "' onClick='" + args.buttons[button_i].onClick +
                           "'>Edit</button>");
            last_was_aed = true;
        } else if (args.buttons[button_i].value == "Delete") {
            document.write("<button class='deleteAction' id='" + args.table_id +
                           suffix + button_i + "' onClick='" + args.buttons[button_i].onClick +
                           "'>Delete</button>");
            last_was_aed = true;
        } else if (args.buttons[button_i].value == "Terminate") {
            document.write("<button class='deleteAction' id='" + args.table_id +
                           suffix + button_i + "' onClick='" + args.buttons[button_i].onClick +
                           "'>Terminate</button>");
            last_was_aed = true;
        } else {
            if (last_was_aed) {
                showDividingLine("dividingLineNoLeft");
                styleStr = "";
            } else if (button_i != 0) {
                styleStr = " style='margin-left:6px'"
            } else {
                styleStr = "";
            }
            last_was_aed = false;
            document.write("<input class='tblButton' type='submit' " +
                           "name='" + args.table_id + suffix + button_i + "' " +
                           "id='" + args.table_id + suffix + button_i + "' " +
                           "onClick='" + args.buttons[button_i].onClick + "' " +
                           "value='" + args.buttons[button_i].value + "' " +
                           "title='" + args.buttons[button_i].value + "' " +
                           styleStr + " />");
        }
    }
}
function needsNowrap(args, col)
{
    if (col == 'obj_name' || (args.col_meta  && args.col_meta[col] && args.col_meta[col]['nowrap'])) {
        return true;
    }
    return false;
}
function isNullEmptyOrNone(arg)
{
    return (arg == null || arg == "" || arg == "none");
}
function showSingle(paged) {
    if (isNullEmptyOrNone(paged.showsingle)) {
        return false;
    }
    if (isNullEmptyOrNone(paged.next) && isNullEmptyOrNone(paged.last) &&
        isNullEmptyOrNone(paged.first) && isNullEmptyOrNone(paged.prev)) {
        return false;
    }
    return true;
}
function showDividingLine(lineClass)
{
    if (!lineClass) {
        lineClass = "dividingLine";
    }
    showTransparentPng({imgPath:'/Images/oracle/table/table_actions_divLine.png', width:2, height:19, classname:lineClass, alt:''});
}
function ht_writePrefsAndCustFilters(args)
{
    var table_id = args.table_id;
    var stripeRows = args.stripeRows;
    if (args.prefs) {
        document.write("  <tr class='tablePrefsRow' id='" + table_id + "PrefsRow' style='display:none'>\n");
        document.write("    <td colspan='" + args.numColumns + "'>\n");
        var prefnumrows = (args.prefs.numrows ? args.prefs.numrows :
                          (args.prefs.defaultnumrows ?  args.prefs.defaultnumrows :
                          "25"));
        document.write("<div style='display:none;' class='' id='" + table_id + "prefs'>\n");
        document.write("  <div class='tableSettingsPanel'>\n");
        document.write("    <div class='tableHeaderLabel'>Table Preferences</div>\n");
        document.write("    <div class='' id='" + table_id + "prefs_content'>\n");
        document.write("      <table border='0' cellpadding='0' cellspacing='0' role='presentation'>\n");
        document.write("        <tbody><tr>\n");
        document.write("          <td class=''><span class='tableHeaderLabel'>\n");
        document.write("            <label for='NumRows'>Rows Per Page:&nbsp;</label>\n");
        document.write("          </span></td>\n");
        document.write("          <td class='filterValueField'>\n");
        document.write("            <input name='NumRows' id='NumRows' " +
                       "value='" + prefnumrows + "' size='5' maxlength='3' type='text' " +
                       "onkeypress='return ht_tblKey(event, \"" + args.prefs.action + "\", \"0123456789\");' />\n");
        document.write("          </td>\n");
        document.write("        </tr></tbody>\n");
        document.write("      </table>\n");
        document.write("      <div class='tableSettingsButtons'>\n");
        document.write("        <input class='tblButton' name='ok_pref' id='ok_pref' value='OK'\n");
        document.write("               onclick='" + args.prefs.action + "'\n");
        document.write("               title='Apply Changes' type='submit' />\n");
        document.write("        <input class='tblButton' name='cancel_pref' id='cancel_pref' value='Cancel'\n");
        document.write("               onclick='ht_togglePane(\"" + table_id + "\",\"empty\");\n");
        document.write("               ht_deselectAll({table_id:\"" + table_id + "\", stripeRows:" + stripeRows + "});'\n");
        document.write("               title='Cancel Changes' type='reset' />\n");
        document.write("      </div>\n");
        document.write("    </div>\n");
        document.write("  </div>\n");
        document.write("</div>\n");
    }
    if (args.filter && args.filter.custom) {
        document.write("<div style='display:none;' class='' id='" + table_id + "custfltr'>\n");
        document.write("    <div class='tableSettingsPanel'>\n");
        document.write("     <div class='tableHeaderLabel'>Custom Filter</div>\n");
        document.write("     <div class='' id='" + table_id + "custfltr_content'>\n");
        if (args.filter.custom.help) {
            document.write("<span class=''>" + args.filter.custom.help + "<br /><br /></span>\n");
        }
        document.write("      <table border='0' cellpadding='0' cellspacing='0' role='presentation'>\n");
        document.write("      <tbody>\n");
        var twocols = (args.filter.custom.inputs.length >= 4);
        for (var i=0; i<args.filter.custom.inputs.length; i++) {
            var input = args.filter.custom.inputs[i];
            if (!twocols || !(i%2)) {
                document.write("    <tr>\n");
            }
            document.write("      <td class=''><span class='tableHeaderLabel'>\n");
            document.write("        <label for='" + input.id + "'>" + input.name + ":</label>\n");
            document.write("      </span></td>\n");
            document.write("      <td class='filterValueField'>\n");
            if (input.type == "text") {
                document.write("    <input name='" + input.id + "' class='' id='" + input.id + "' " +
                               "value='" + input.value + "' type='text' " +
                               "size='" + (input.size ? input.size : "30") + "' " +
                               "maxlength='" + (input.maxlength ? input.maxlength : "127") + "' " +
                               "onkeypress='return ht_tblKey(event, \"" + args.filter.custom.action + "\");' />\n");
            } else if (input.type == "select") {
                showCsvAsDropdown({id:input.id, csv:input.csv, sel:input.value,
                                   empty_txt:"&mdash;&nbsp;Select&nbsp;&mdash;", empty_val:""});
            }
            document.write("      </td>\n");
            if (!twocols || (i%2)) {
                document.write("    </tr>\n");
            }
        }
        document.write("      </tbody>\n");
        document.write("      </table>\n");
        document.write("     <div class='tableSettingsButton'>\n");
        document.write("      <input class='tblButton' name='ok_filter' id='ok_filter' value='OK'\n");
        document.write("       onclick='" + args.filter.custom.action + "'\n");
        document.write("       title='Apply Changes' type='submit' />\n");
        document.write("      <input class='tblButton' name='cancel_filter' id='cancel_filter' value='Cancel'\n");
        document.write("       onclick='ht_togglePane(\"" + table_id + "\",\"empty\");\n");
        document.write("                ht_deselectAll({table_id:\"" + table_id + "\"});'\n");
        document.write("       title='Cancel Changes' type='reset' />\n");
        document.write("     </div>\n");
        document.write("     </div>\n");
        document.write(" </div>\n");
        document.write("</div>\n");
    }
    if (args.prefs) {
        document.write("    </td>\n");
        document.write("  </tr>\n");
    }
}
function getHeadId(args)
{
    if (args.id == "col") {
        return (args.table + "_col_" + args.col);
    } else if (args.id == "row") {
        return (args.table + "_Row" + args.row);
    } else if (args.id == "rowid") {
        return (args.table + "_Row" + args.row + "_cell");
    } else if (args.id == "nv") {
        return (args.table + "_Row" + args.row + "_col_" + args.col + "_nv_" + args.nv);
    }
    alert("Internal Error: " + args.id);
}
function tableHasHead(args) {
    return (args.heading || args.buttons ||
            args.dropdown || args.filter ||
            args.prefs || args.paged ||
            (args.columnnames != undefined && (args.useColNames == undefined || args.useColNames == true)));
}
function showAsTable(args) {
    var table_id = "ht_Tbl";
    var numColumns = 0;
    var numRows = 0;
    var nvColSpan;
    var stripeRows = (args.stripe == undefined || args.stripe == "rows");
    var stripeColumns = (args.stripe == "columns");
    var editableTbl = false;
    var wroteTableCtrlRow = false;
    var rowClass = "";
    var tableSummaryOrRole = "";
    var tableArray;
    var initcol;
    var cellIdOrHead;
    var headTxt;
    var idTxt;
    if (args.ary) {
        tableArray = args.ary;
    } else {
        tableArray = new Array();
    }
    if (args.summary) {
        tableSummaryOrRole = " summary='" + args.summary + "'";
    } else if (args.layoutTable) {
        tableSummaryOrRole = " role='presentation'";
    }
    if (args.skipRows && args.matchRows) {
        alert("Developer Error: Can only specify skipRows _or_ matchRows for the '" + args.title + "' table.");
    } else if (args.skipRows || args.matchRows) {
        var subsetRows = args.skipRows ? args.skipRows : args.matchRows;
        var subsetField, subsetValue;
        for (var theSubset in subsetRows) {
            subsetField = theSubset;
            subsetValue = subsetRows[theSubset];
            if (args.skipRows) {
                tableArray = tableArray.skipData(subsetField, subsetValue);
            } else {
                tableArray = tableArray.matchData(subsetField, subsetValue);
            }
        }
    }
    if (args.pv != null && args.pv == true) {
        var jcount=0;
        var pvary = new Array;
        for (var i=0; i<tableArray.length; ++i) {
            for (var j in tableArray[i]['data']) {
                if (j != args.skipColumn && j != "row_index" && j != "obj_name") {
                    var prop;
                    if (args.columnnames && args.columnnames[j]) {
                        prop = args.columnnames[j];
                    } else {
                        prop = make_pretty_propname(j);
                    }
                    pvary[jcount++] = { data: { Property:prop, Value:tableArray[i]['data'][j] } };
                }
            }
        }
        args.ary = pvary;
        args.pv = null;
        args.col_meta = {Property:{nowrap:'nowrap'}};
        showAsTable(args);
        return;
    }
    if (args.id != null) {
        table_id += String(args.id).replace(/[ _]/g,"").make_html_id();
    } else {
        table_id += String(args.title).replace(/[ _]/g,"").make_html_id();
    }
    numRows = tableArray.length;
    if (numRows > 0) {
        if (args.collapse && args.collapse.columnnames) {
            numColumns = args.collapse.columnnames.length;
        } else {
            for (var j in tableArray[0]['data']) {
                currHeader = j;
                if (args.columnnames && args.columnnames[j]) {
                    currHeader = args.columnnames[j];
                } else if (args.hideunnamedcols == 'true') {
                    currHeader = "do_hide";
                }
                if (args.skipColumn != currHeader && currHeader != "row_index" && currHeader != "do_hide" && currHeader != args.summaryRow) {
                    numColumns++;
                }
            }
        }
    } else {
        if (args.collapse && args.collapse.columnnames) {
            numColumns = args.collapse.columnnames.length;
        } else {
            if (args.columnnames) {
                for (var j in args.columnnames) {
                    currHeader = args.columnnames[j];
                    if (currHeader != args.skipColumn && currHeader != "row_index" && currHeader != "do_hide") {
                        numColumns++;
                    }
                }
            }
        }
    }
    if (args.select) {
        numColumns++;
    }
    if (args.nvColumns) {
        numColumns += args.nvColumns.length;
    }
    var width;
    if (args.width != null)
        width = args.width;
    else
        width='100%';
    document.write("<div class=''>\n");
    document.write("<form action='#' onsubmit='return false;' name='" + table_id + "_Form'" +
                   " id='" + table_id + "_Form" + "'" +
                   " style='margin-top:0px; margin-bottom:0px;'>\n");
    if (!args.nocaption && args.layoutTable) {
        document.write("<div class='tableCaption'>" + args.title + "</div>\n");
    }
    tableClass = "Table";
    if (args.stripe == "table") {
        tableClass += " tableShaded";
    }
    document.write("<table width='" + width + "' " +
                   "border='1' " +
                   "cellpadding='0' " +
                   "cellspacing='0' " +
                   "class='"+ tableClass + "' " +
                   "id='" + table_id + "'" +
                   tableSummaryOrRole +
                   ">\n");
    if (!args.nocaption && !args.layoutTable) {
        document.write("<caption class='tableCaption'>" + args.title + "</caption>\n");
    } else if (args.nocaption && args.title) {
        document.write("<caption class='hiddenContent'>" + args.title + "</caption>\n");
    }
    if (tableHasHead(args)) {
        document.write("<thead>\n");
        if (args.heading) {
            document.write("  <tr class='tableHeadingRow'>\n");
            document.write("    <td colspan='" + numColumns + "'>\n");
            for (var heading_count = 0; heading_count < args.heading.length; heading_count++) {
                var linkStartStr = "";
                var linkEndStr = "";
                if (args.heading[heading_count].link != undefined) {
                    linkStartStr = "<a " + args.heading[heading_count].link + " class='tableActionLink'>";
                    linkEndStr = "<\/a>";
                }
                document.write(linkStartStr + args.heading[heading_count].name + ":<div class='spanValue'>" + args.heading[heading_count].value +
                               linkEndStr + "<\/div>");
            }
            document.write("<\/td><\/tr>\n");
        }
        if (hasItems(args.buttons)) {
            if (!wroteTableCtrlRow) {
                writeTableCtrlRow(numColumns);
                wroteTableCtrlRow = true;
            }
            writeTableButtons({buttons:args.buttons, table_id:table_id, set:'top'});
        }
        var dropdownDisplay = 'false';
        if (args.dropdown) {
            for (var i=0; i<numRows; i++) {
                if (tableArray[i]['meta']['settable'] == 'true') {
                    dropdownDisplay = 'true';
                    break;
                }
                if (tableArray[i]['data'] && tableArray[i]['data']['crows']) {
                    var c = tableArray[i]['data']['crows'];
                    for (var j=0; j < c.length; j++) {
                        if (c[j]['meta']['settable'] == 'true') {
                            dropdownDisplay = 'true';
                            break;
                        }
                    }
                }
                if (dropdownDisplay == 'true') {
                    break;
                }
            }
            if (dropdownDisplay == 'true') {
                if (!wroteTableCtrlRow) {
                    writeTableCtrlRow(numColumns);
                    wroteTableCtrlRow = true;
                }
                var selOnChange = "var selectedid=parseInt(this.selectedIndex)-1; " +
                        "ht_resetDropdown(\"" + table_id + "_DropdownLabel\"); " +
                        "eval(getElementById(\"" + table_id + "_DropdownOption\" + selectedid).value); ";
                var selLabel = args.dropdown_meta && args.dropdown_meta.label ? args.dropdown_meta.label : "&mdash;&nbsp;Actions&nbsp;&mdash;";
                var optTable = new Array();
                optTable.push({id:table_id + "_DropdownLabel", state:"selectedAction", text:selLabel});
                for (var dropdown_i=0; dropdown_i < args.dropdown.length; dropdown_i++) {
                    var my_dropdownoption_id = table_id + "_DropdownOption" + dropdown_i;
                    if (typeof(args.dropdown[dropdown_i].row_action_name) != 'undefined') {
                        row_action_names[table_id + "_" + args.dropdown[dropdown_i].row_action_name] = my_dropdownoption_id;
                    }
                    var optValue = String(args.dropdown[dropdown_i].action).replace(/\'/g, '\"');
                    optTable.push({id:my_dropdownoption_id, value:optValue, state:"disabledAction", text:args.dropdown[dropdown_i].value});
                }
                document.write("<label for='" + table_id + "_Dropdown' class='hiddenContent'>Action</label>");
                showAsDropdown({id:table_id + "_Dropdown", ary:optTable, state:"action", onChange:selOnChange});
            }
        }
        if (args.filter) {
            if (!wroteTableCtrlRow) {
                writeTableCtrlRow(numColumns);
                wroteTableCtrlRow = true;
            }
            if (hasItems(args.buttons) || dropdownDisplay == 'true') {
                showDividingLine();
            }
            var mnustate = "action";
            var fltrOpts = new Array();
            var fltr_i = 0;
            var option_id_prefix = table_id + "filtermenu_Opt";
            if (args.filter.predefined) {
                for (var fltr_i = 0; fltr_i < args.filter.predefined.length; fltr_i++) {
                    var state = "action";
                    if (args.filter.sel && args.filter.sel == fltr_i) {
                        state = "selectedaction";
                    }
                    fltrOpts.push({id:option_id_prefix+fltr_i, value:args.filter.predefined[fltr_i].action, state:state, text:args.filter.predefined[fltr_i].name});
                }
            }
            if (args.filter.custom) {
                fltrOpts.push({id:option_id_prefix+fltr_i, value:"", state:"disabledAction", text:"&mdash;&mdash;&mdash;&mdash;&mdash;&mdash;"});
                fltr_i++;
                var val = "var tblid = \"" + table_id + "\"; ht_togglePane(tblid, \"custfltr\");";
                if (args.filter.sel && args.filter.sel == fltr_i) {
                    mnustate = "filtered";
                    fltrOpts.push({id:option_id_prefix+fltr_i, value:"", state:"selectedaction", text:"Custom Filter Applied"});
                    fltr_i++;
                }
                fltrOpts.push({id:option_id_prefix+fltr_i, value:val, state:"action", text:"Custom Filter..."});
            }
            var selOnChange = "var selectedid=parseInt(this.selectedIndex); " +
                    "eval(getElementById(\"" + table_id + "filtermenu_Opt\" + selectedid).value); ";
            document.write("<label for='" + table_id + "filtermenu'>" +
                    "<span class='tableHeaderLabel'>Filter:</span></label>\n");
            showAsDropdown({id:table_id + "filtermenu", ary:fltrOpts, state:mnustate, onChange:selOnChange, className:"tableDropdown"});
        }
        if (args.prefs) {
            if (!wroteTableCtrlRow) {
                writeTableCtrlRow(numColumns);
                wroteTableCtrlRow = true;
            }
            if (hasItems(args.buttons) || args.dropdown || args.filter) {
                showDividingLine();
            }
            document.write("<a href='#' onclick='ht_togglePane(\"" + table_id + "\", \"prefs\");return false;' title='Show Table View Preferences'>");
            showTransparentPng({imgPath:'/Images/oracle/icons/view_prefs_icon.png',
                                width:13, height:17, alt:'Show Table View Preferences',
                                title:'Show Table View Preferences', valign:'text-top'});
            document.write("<\/a>\n");
        }
        if (args.paged) {
            if (!wroteTableCtrlRow) {
                writeTableCtrlRow(numColumns);
                wroteTableCtrlRow = true;
            }
            var separatorflag = false;
            if (args.paged.showmulti && args.paged.showmulti != "none") {
                if (hasItems(args.buttons) || args.dropdown || args.filter || args.prefs) {
                    showDividingLine();
                    separatorflag = true;
                }
                document.write("<a href='#' onclick=\"" + args.paged.showmulti + ";return false;\" title='Show Data in Multiple Pages'>");
                showTransparentPng({imgPath:'/Images/oracle/icons/multiplePage_icon.png', width:17, height:17,
                                    alt:'Show Data in Multiple Pages', title:'Show Data in Multiple Pages',
                                    valign:'text-top'});
                document.write("<\/a>");
            }
            if (showSingle(args.paged)) {
                if (!wroteTableCtrlRow) {
                    writeTableCtrlRow(numColumns);
                    wroteTableCtrlRow = true;
                }
                if ((hasItems(args.buttons) || args.dropdown || args.filter || args.prefs) &&
                    (!separatorflag)) {
                    showDividingLine();
                }
                document.write("<a href='#' title='Show Data in a Single Page' onclick=\"" + args.paged.showsingle + "return false;\" >");
                showTransparentPng({imgPath:'/Images/oracle/icons/singlePage_icon.png', width:15, height:17,
                                    alt:'Show Data in a Single Page', title:'Show Data in a Single Page',
                                    valign:'text-top'});
                document.write("<\/a>\n");
            }
        }
        if (wroteTableCtrlRow) {
            document.write("   \n</td>\n  </tr>\n");
        }
        if (args.prefs || (args.filter && args.filter.custom)) {
            ht_writePrefsAndCustFilters({filter:args.filter,
                                         prefs:args.prefs,
                                         table_id:table_id,
                                         stripeRows:stripeRows,
                                         numColumns:numColumns});
        }
        var usecolnameorder = args.usecolnameorder;
        if (typeof(usecolnameorder) != "boolean") {
            DefaultInit(usecolnameorder, "false").toBoolean();
        }
        var useColNames = (args.useColNames != false);
        if (useColNames) {
            var colNum = 0;
            if (numRows == 0) {
                usecolnameorder = true;
            }
            document.write("  <tr class='tableHeaderRow'>\n");
            if (args.select != null) {
                document.write("    <th class='rowSelect' scope='col'></th>\n");
            }
            if (args.collapse && args.collapse.columnnames) {
                for (var j=0; j<args.collapse.columnnames.length; j++) {
                    currHeader = args.collapse.columnnames[j];
                    if (currHeader == "") {
                        currHeader = "&nbsp;";
                    }
                    document.write("    <th align='left' ");
                    if (j == 0) {
                        document.write(" class='tableHeaderFirst' width='25%' ");
                    } else {
                        document.write(" class='tableHeader' ");
                    }
                    document.write("scope='col'>" + currHeader + "</th>\n");
                }
            } else if (usecolnameorder) {
                var headerClass;
                for (var j in args.columnnames) {
                    currHeader = args.columnnames[j];
                    if (currHeader == "") {
                        currHeader = "&nbsp;";
                    }
                    if (colNum == 0) {
                        headerClass = "tableHeaderFirst";
                    } else {
                        headerClass = "tableHeader";
                    }
                    idTxt = getHeadId({id:"col", table:table_id, col:colNum});
                    document.write("    <th align='left' class='" + headerClass + "' id='" + idTxt + "' " +
                                   "scope='col'><div class='tableCellNoWrap'>" + currHeader + "</div></th>\n");
                    colNum++;
                }
            } else {
                for (var j in tableArray[0]['data']) {
                    var widthStr = "";
                    currHeader = j;
                    if (args.columnnames && args.columnnames[j]) {
                        currHeader = args.columnnames[j];
                    } else if (args.hideunnamedcols == 'true') {
                        currHeader = "do_hide";
                    }
                    if (currHeader != "row_index" && currHeader != "do_hide" && (args.skipColumn == undefined || args.skipColumn.indexOf(currHeader) == -1) && args.summaryRow != currHeader) {
                        if (args.nvColumns && args.nvColumns.indexOf(j) != -1) {
                            nvColSpan = "colspan='2'";
                        } else {
                            nvColSpan = "";
                        }
                        if (j == "obj_name") {
                            widthStr = " width='1%'";
                        }
                        if (currHeader == "skip" || currHeader == "none") {
                            currHeader = "&nbsp;";
                        }
                        if (colNum == 0) {
                            headerClass = "tableHeaderFirst";
                        } else {
                            headerClass = "tableHeader";
                        }
                        idTxt = getHeadId({id:"col", table:table_id, col:colNum});
                        document.write("    <th align='left' class='" + headerClass + "' " + widthStr + nvColSpan + " id='" + idTxt + "' scope='col'><div class='tableCellNoWrap'>" + currHeader + "</div></th>\n");
                        colNum++;
                    }
                }
            }
            document.write("  </tr>\n");
        }
        document.write("    </thead>\n");
    }
    if (args.paged || (numRows >= 15 && (args.buttons || args.dropdown))) {
        document.write("    <tfoot>\n");
        wroteTableCtrlRow = false;
        if (hasItems(args.buttons)) {
            writeTableCtrlRow(numColumns);
            wroteTableCtrlRow = true;
            writeTableButtons({buttons:args.buttons, table_id:table_id, set:'bottom'});
        }
        dropdownDisplay = 'false';
        if (args.dropdown) {
            for (var i=0; i<numRows; i++) {
                if (tableArray[i]['meta']['settable'] == 'true') {
                    dropdownDisplay = 'true';
                    break;
                }
            }
            if (dropdownDisplay == 'true') {
                if (!wroteTableCtrlRow) {
                    writeTableCtrlRow(numColumns);
                    wroteTableCtrlRow = true;
                }
                var selOnChange = "var selectedid=parseInt(this.selectedIndex)-1; " +
                        "ht_resetDropdown(\"" + table_id + "_Dropdown2Label\"); " +
                        "eval(getElementById(\"" + table_id + "_Dropdown2Option\" + selectedid).value); ";
                var selLabel = args.dropdown_meta && args.dropdown_meta.label ? args.dropdown_meta.label : "&mdash;&nbsp;Actions&nbsp;&mdash;";
                var optTable = new Array();
                optTable.push({id:table_id + "_Dropdown2Label", state:"selectedAction", text:selLabel});
                for (var dropdown_i=0; dropdown_i < args.dropdown.length; dropdown_i++) {
                    var my_dropdownoption_id = table_id + "_Dropdown2Option" + dropdown_i;
                    if (typeof(args.dropdown[dropdown_i].row_action_name) != 'undefined') {
                        row_action_names2[table_id + "_" + args.dropdown[dropdown_i].row_action_name] = my_dropdownoption_id;
                    }
                    var optValue = String(args.dropdown[dropdown_i].action).replace(/\'/g, '\"');
                    optTable.push({id:my_dropdownoption_id, value:optValue, state:"disabledAction", text:args.dropdown[dropdown_i].value});
                }
                showAsDropdown({id:table_id + "_Dropdown2", ary:optTable, state:"action", onChange:selOnChange});
            }
        }
        function drawPageBtn(pos, action) {
            var Pos = {start:"First", back:"Previous", next:"Next", end:"Last"};
            if (action == null || action == "" || action == "none") {
                showTransparentPng({imgPath:'/Images/oracle/icons/table_' + pos + '_icon_dis.png', width:20, height:19,
                                   alt:Pos[pos] + ' Page Disabled', title:Pos[pos] + ' Page Disabled', classname:'tableButton'+Pos[pos]});
            } else {
                document.write("<a href='#' onclick=\"" + action + ";return false;\">");
                showTransparentPng({imgPath:'/Images/oracle/icons/table_' + pos + '_icon.png', width:20, height:19,
                                   alt:'Go To ' + Pos[pos] + ' Page', title:'Go To ' + Pos[pos] + ' Page', classname:'tableButton'+Pos[pos]});
                document.write("</a>");
            }
        }
        if (args.paged) {
            if (!wroteTableCtrlRow) {
                writeTableCtrlRow(numColumns);
                wroteTableCtrlRow = true;
            }
            if (hasItems(args.buttons) || dropdownDisplay == 'true') {
                showDividingLine();
            }
            if (args.paged.first) {
                drawPageBtn("start", args.paged.first);
            }
            if (args.paged.prev) {
                drawPageBtn("back", args.paged.prev);
            }
            if (args.paged.next) {
                drawPageBtn("next", args.paged.next);
            }
            if (args.paged.last) {
                drawPageBtn("end", args.paged.last);
            }
            if (args.paged.next || args.paged.last) {
                showDividingLine();
            }
            if (args.paged.showmulti && args.paged.showmulti != "none") {
                document.write("<a href='#' onclick=\"" + args.paged.showmulti + ";return false;\" title='Show Data in Multiple Pages'>");
                showTransparentPng({imgPath:'/Images/oracle/icons/multiplePage_icon.png', width:17, height:17,
                                    alt:'Show Data in Multiple Pages', title:'Show Data in Multiple Pages',
                                    valign:'text-top'});
                document.write("<\/a>");
            }
            if (showSingle(args.paged)) {
                document.write("<a href='#' title='Show Data in a Single Page' onclick=\"" + args.paged.showsingle + "return false;\" >");
                showTransparentPng({imgPath:'/Images/oracle/icons/singlePage_icon.png', width:15, height:17,
                                    alt:'Show Data in a Single Page', title:'Show Data in a Single Page',
                                    valign:'text-top'});
                document.write("<\/a>\n");
            }
        }
        if (wroteTableCtrlRow) {
            document.write("   \n</td>\n  </tr>\n");
        }
        document.write("    </tfoot>\n");
    }
    document.write("    <tbody>\n");
    if (numRows < 1) {
        if (stripeRows) {
            rowClass = " class='tableRowEven'";
        }
        document.write("<tr" + rowClass + ">");
        document.write("    <td class='tableCell tableCellNoLeft' colspan='" + numColumns + "'>" +
                       "No Items To Display<\/span><\/td>\n");
        document.write("  <\/tr>\n");
        if (hasItems(args.buttons)) {
            ht_handleButtonsEmptyTable({buttons:args.buttons, table_id:table_id});
        }
    }
    for (var i=0; i<numRows; ++i) {
        rowClass = "";
        var maxNVList = 1;
        if (args.nvColumns) {
            for (var j in args.nvColumns) {
                var idx = args.nvColumns[j];
                var nvList;
                if (tableArray[i]['data'][idx]) {
                    nvList = tableArray[i]['data'][idx].length;
                    if (maxNVList < nvList) {
                       maxNVList = nvList;
                    }
                }
            }
        }
        if (stripeRows) {
            rowClass = " class='" + getStripeClass(i);
            if (args.select) {
                rowClass += " tableRowAction";
            }
            rowClass += "'";
        }
        document.write("  <tr id='" + table_id + "_Row" + i + "'" + rowClass + " \n");
        if (tableArray[i]['meta'] && tableArray[i]['meta']['hidden']) {
            document.write(" style='visibility:hidden'");
        }
        var thisGroup = "";
        if (args.select != null) {
            editableTbl = true;
            var row_actions = tableArray[i]['meta']['row_actions'];
            var thisRow = tableArray[i]['data']['row_index'];
            var enabledButtons = tableArray[i]['meta']['enabled_buttons']
            if (enabledButtons == undefined) {
                enabledButtons = "ALL";
            }
            if (tableArray[i]['meta']['settable'] == "false") {
                enabledButtons = "";
            }
            if (row_actions) {
                row_actions = "'" + row_actions + "'";
            }
            document.write(
                "onclick=\"ht_processTableClick(" +
                "{" +
                "table_id:'" + table_id + "', " +
                "rowdata:'" + thisRow + "', " +
                "row_actions:" + row_actions + ", " +
                "enabled_buttons:'" + enabledButtons + "', " +
                "row:'" + i + "', " +
                "stripeRows:" + stripeRows +
                "});\" ");
            document.write("title = 'Click to select the row.'");
            document.write(" aria-selected='false'");
            document.write(" aria-label='unselected'");
            document.write(">\n");
            document.write("    <td align='center' class='rowSelect' scope='row'>\n");
            document.write("<input type='radio'");
            document.write(
                        "name='selrow' " +
                        "id='" + table_id + "_SelRow" + i + "' " +
                        "value='" + i + "' " +
                        " />\n");
            document.write("</td>\n");
        } else {
            document.write(">\n");
        }
        if ((args.collapse) && (tableArray[i]['meta']['docollapse'] == 'true')) {
            thisGroup = getHeadId({id:"row", table:table_id, row:i});
            var cellClass = "tableCell tableCellNoLeft";
            if (i == 0) {
                cellClass += "tableCellNoTop"
            }
            if (args.layoutTable) {
                headTxt= "";
            } else {
                headTxt = " headers ='" + getHeadId({id:"col", table:table_id, col:0}) + "'";
            }
            if (tableArray[i]['data']['crows']) {
                document.write("   <td id='" + thisGroup + "_cell' class='tableCellCollapsible' align='left' onclick='return ht_toggleCRows(this.parentNode);' title='Click to Expand Group'" + headTxt + "'>\n");
                document.write("    <span id='" + thisGroup + "_name' class='collapsibleCell'>\n");
                document.write(showTransparentPngHtml({imgPath:'/Images/oracle/table/table_expand_icon.png', alt:'Expand', classname:'tableCollapsibleIcon', valign:'middle', id:thisGroup + '_icon', height:7, width:7}));
                document.write(tableArray[i]['data'][args.collapse.group_obj_label].replace(/_/g, " "));
                document.write("    </span>\n");
            } else {
                document.write("    <td class='tableCellNoCollapse' align='left' id='" + thisGroup + "_cell'" + headTxt + "><div class='tableCellNoWrap'>\n");
                document.write(tableArray[i]['data'][args.collapse.group_obj_label].replace(/_/g, " ") + "</div>");
            }
            document.write("   </td>\n");
            colNum = 1;
            for (var j in tableArray[i]['data']) {
                if (j != args.skipColumn && j != "row_index" && j != "obj_name") {
                    if (args.columnnames && args.columnnames[j]) {
                        var headerVal;
                        if (args.layoutTable) {
                            headerVal = "";
                        } else {
                            headerVal = " headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + " " + getHeadId({id:'col', table:table_id, col:colNum}) + "'";
                        }
                        document.write("   <td class='tableCell' align='left'" + headerVal + ">\n");
                        colNum++;
                        if (args.statusColumn && args.statusColumn == j) {
                            handleHealthStatus(tableArray[i]['data'][j]);
                        } else {
                            if (needsNowrap(args, j)) {
                                document.write("<div class='tableCellNoWrap'>" + tableArray[i]['data'][j] + "</div>");
                            } else {
                                document.write(tableArray[i]['data'][j]);
                            }
                        }
                        document.write("   <\/td>\n");
                    }
                }
            }
        }
        colNum = 0;
        for (var j in (usecolnameorder ? args.columnnames : tableArray[i]['data'])) {
            var paddingStr = "";
            if (maxNVList > 1) {
                if (args.col_meta && args.col_meta[j] && typeof(args.col_meta[j]['padding']) != "undefined") {
                    paddingStr = " style='padding-bottom:" + args.col_meta[j]['padding'] + "px'";
                }
            }
            if (j == 'row_index' || (args.skipColumn && args.skipColumn.indexOf(j) != -1) || args.summaryRow == j) {
                continue;
            } else if (args.columnnames && args.columnnames[j] == undefined &&
                       j != 'crows' && args.hideunnamedcols == "true") {
                continue;
            } else if (args.nvColumns && args.nvColumns.indexOf(j) != -1) {
                var cellVal;
                var nvp = tableArray[i]['data'][j];
                document.write("\n    <td ");
                if (args.col_meta && args.col_meta[j] && args.col_meta[j]['namewidth']) {
                    document.write("width='" + args.col_meta[j]['namewidth'] + "' ");
                }
                cellClass = "tableCell";
                if (colNum == 0) {
                    cellClass += " tableCellNoLeft";
                }
                if (stripeColumns) {
                    cellClass += " " + getCellShade(colNum);
                }
                if (i == 0 && useColNames == false) {
                    cellClass += " tableCellNoTop";
                }
                idTxt = getHeadId({id:'nv', table:table_id, row:i, col:colNum, nv:0});
                if (nvp[0].value == "" && nvp[0].name == "") {
                    headTxt = "";
                    cellVal = "&nbsp;"
                } else {
                    if (args.layoutTable) {
                        headTxt = "";
                    } else {
                        headTxt = " headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + " " + getHeadId({id:'col', table:table_id, col:colNum}) + "'";
                    }
                    cellVal = nvp[0].name;
                }
                document.write("align='left' valign='top' class='" + cellClass + "' scope='row' style='padding-right:0px;' id='" + idTxt + "'" + headTxt + ">\n");
                if (nvp[0].nonamespan) {
                    document.write(cellVal);
                } else {
                    document.write("<div class='tableCellNoWrap'>" + cellVal + "</div></td>\n");
                }
                if (nvp[0].value != undefined) {
                    document.write("\n    <td ");
                    if (args.col_meta && args.col_meta[j] && args.col_meta[j]['valwidth']) {
                        document.write("width='" + args.col_meta[j]['valwidth'] + "' ");
                    }
                    cellClass = "tableCell tableCellNoLeft";
                    if (i == 0 && useColNames == false) {
                        cellClass += " tableCellNoTop";
                    }
                    if (stripeColumns) {
                        cellClass += " " + getCellShade(colNum);
                    }
                    if ((nvp[0].value == "" && nvp[0].name == "")) {
                        headTxt = "";
                        cellVal = "&nbsp;"
                    } else {
                        if (args.layoutTable) {
                            headTxt = "";
                        } else {
                            headTxt = " headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + " " + getHeadId({id:'col', table:table_id, col:colNum}) + " " + getHeadId({id:'nv', table:table_id, row:i, col:colNum, nv:0}) + "'";
                        }
                        cellVal = nvp[0].value;
                    }
                    document.write("align='left' valign='top' class='" + cellClass + "'" + headTxt + ">\n");
                    document.write(cellVal + "</td>\n");
                    colNum++;
               }
            } else {
                if ((args.collapse) &&
                    (tableArray[i]['meta']['docollapse'] == 'true'))  {
                    if (j != "crows") {
                        continue;
                    }
                } else {
                    if (args.columnnames && args.columnnames[j] == "do_hide") {
                        continue;
                    }
                    document.write("\n    <td");
                    if (args.col_meta && args.col_meta[j] && args.col_meta[j]['width']) {
                        document.write(" width='" + args.col_meta[j]['width'] + "'");
                    }
                    cellClass = "tableCell";
                    if (colNum == 0) {
                        cellClass += " tableCellNoLeft";
                        cellIdOrHead = " id='" + getHeadId({id:'rowid', table:table_id, row:i}) + "'";
                        if (useColNames && !args.layoutTable) {
                            cellIdOrHead += " headers='" + getHeadId({id:'col', table:table_id, col:colNum}) + "' ";
                        }
                    } else {
                        if (args.layoutTable) {
                            cellIdOrHead = "";
                        } else {
                            cellIdOrHead = " headers='" + getHeadId({ id:'rowid', table:table_id, row:i});
                            if (useColNames) {
                                cellIdOrHead += " " + getHeadId({id:'col', table:table_id, col:colNum});
                            }
                            cellIdOrHead += "' ";
                        }
                    }
                    if (stripeColumns) {
                        cellClass += " " + getCellShade(colNum);
                    }
                    if (i == 0 && useColNames == false) {
                        cellClass += " tableCellNoTop";
                    }
                    if (args.summaryRow) {
                        cellClass += " tableCellDivider";
                    }
                    document.write(" align='left' valign='top'" + cellIdOrHead + " class='" + cellClass + "'>\n");
                    colNum++;
                }
                var d;
                if ((args.collapse) &&
                    (tableArray[i]['meta']['docollapse'] == 'true')) {
                    var collary = tableArray[i]['data']['crows'];
                    for (var k=0; k<collary.length; ++k) {
                        document.write("  <tr id='" + thisGroup + "_sr" + k + "_Row' style='display:none;' class='rowClassSubRow'");
                        if (args.select != null && (collary[k] && collary[k]['meta'] && collary[k]['meta']['action'] && collary[k]['meta']['action'] != '')) {
                            var crow_actions = collary[k]['meta']['row_actions'];
                            if (crow_actions) {
                                crow_actions = "'" + crow_actions + "'";
                            }
                            document.write(
                                "onclick=\"ht_processTableClick({" +
                                "table_id:'" + table_id + "', " +
                                "enabled_buttons:'" + enabledButtons + "', " +
                                "row:'" + i + "', " +
                                "stripeRows:" + stripeRows + ", " +
                                "row_actions:" + crow_actions + ", " +
                                "crow:'" + thisGroup + "_sr" + k + "'" +
                                "});\">\n");
                            document.write("    <td align='center' class='' scope='row' style='display:none;'>\n");
                            document.write("<input ");
                            document.write(
                                "type=\"radio\" " +
                                "name='selrow' " +
                                "id='" + thisGroup + "_sr" + k + "_SelRow' " +
                                "value='" + i + ":" + k + "' " +
                                " />\n");
                        } else {
                            document.write(">\n");
                        }
                        document.write("\n    </td>\n");
                        initcol = true;
                        colNum = 0;
                        for (var l in args.columnnames) {
                            if ((!tableArray[0].data[l]) &&
                                (args.collapse.columnnames == undefined || !args.collapse.columnnames[l])) {
                                continue;
                            }
                            d = collary[k]['data'][l];
                            if (initcol) {
                                document.write("   <td align='left' valign='top' class='tableCellSubRow' id='" + thisGroup + "_sr" + k + "'");
                                if (!args.layoutTable) {
                                    document.write(" headers='" + thisGroup + "_cell " + getHeadId({id:'col', table:table_id, col:colNum}) + "'");
                                }
                                document.write(">\n");
                            } else {
                                document.write("   <td align='left' valign='top' class='tableCell'");
                                if (!args.layoutTable) {
                                    document.write(" headers='" + thisGroup + "_cell " + thisGroup + "_sr" + k +  " " + getHeadId({id:'col', table:table_id, col:colNum}) + "'");
                                }
                                document.write(">\n");
                            }
                            colNum++
                            if (d == undefined) {
                                document.write("<img src='/Images/sun/2.2/table/empty_table_cell.gif' alt='Not Applicable' />");
                                document.write("</td>\n");
                                continue;
                            }
                            if (initcol) {
                                document.write("    <span class='collapsibleCell' >\n");
                                document.write(d);
                                document.write("    </span>\n");
                                initcol = false;
                            } else {
                                if (args.statusColumn && args.statusColumn == l) {
                                    handleHealthStatus(d);
                                } else {
                                    if (needsNowrap(args, l)) {
                                        document.write("<div class='tableCellNoWrap'>" + d + "</div>");
                                    } else {
                                        document.write(d);
                                    }
                                }
                            }
                            document.write("</td>\n");
                        }
                        document.write("  </tr>\n");
                    }
                } else {
                    d = tableArray[i]['data'][j];
                    if (args.col_link && args.col_link[j] && !tableArray[i]['meta']['nolink']) {
                        document.write("<a href='#' onclick='" +
                          args.col_link[j] + "(\"" + d + "\");" +
                          "return false;'" +
                          " title='Display additional information for " + d + "'" +
                          ">");
                    }
                    if (args.statusColumn && args.statusColumn == j) {
                        handleHealthStatus(d);
                    } else if (d && d.length) {
                        if (needsNowrap(args, j)) {
                            document.write("<div class='tableCellNoWrap'>" + d + "</div>");
                        } else {
                            document.write(d);
                        }
                        if ((args.collapse) &&
                            (tableArray[i]['meta']['docollapse'] == 'false')) {
                            document.write("&nbsp;&nbsp;&nbsp;");
                        }
                    } else {
                        document.write("<img src='/Images/sun/2.2/table/empty_table_cell.gif' alt='Not Applicable' />");
                    }
                    if (args.col_link && args.col_link[j] && !tableArray[i]['meta']['nolink']) {
                        document.write("</a>");
                    }
                    document.write("\n    </td>\n");
                }
            }
        }
        for (var nvCount = 1; nvCount < maxNVList; nvCount++) {
            colNum = 0;
            document.write("<tr" + rowClass + ">\n");
            for (var j in (usecolnameorder ? args.columnnames : tableArray[i]['data'])) {
                if (j == "row_index") {
                    continue;
                }
                if (args.nvColumns.indexOf(j) != -1) {
                    var nvp = tableArray[i]['data'][j];
                    var cellVal;
                    if (nvCount < nvp.length) {
                        cellClass = "tableCell tableCellNoTop";
                        if (colNum == 0) {
                            cellClass += " tableCellNoLeft";
                        }
                        if (stripeColumns) {
                            cellClass += " " + getCellShade(colNum);
                        }
                        if ((nvp[nvCount].value == "" && nvp[nvCount].name == "")) {
                            headTxt = "";
                            cellVal = "&nbsp;"
                        } else {
                            if (args.layoutTable) {
                                headTxt = "";
                            } else {
                                headTxt = " headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + " " +  getHeadId({id:'col', table:table_id, col:colNum}) + "'";
                            }
                            cellVal = nvp[nvCount].name;
                        }
                        idTxt = getHeadId({id:'nv', table:table_id, row:i, col:colNum, nv:nvCount});
                        document.write("<td align='left' valign='top' class='" + cellClass + "' style='padding-right:0px'" + headTxt + " id='" + idTxt + "'>\n");
                        if (nvp[nvCount].nonamespan) {
                            document.write(cellVal);
                        } else {
                            document.write("<div class='tableCellNoWrap'>" + cellVal + "</div>");
                        }
                        document.write("</td>\n");
                        if (nvp[0].value != undefined) {
                            cellClass = "tableCell tableCellNoTop tableCellNoLeft";
                            if (stripeColumns) {
                                cellClass += " " + getCellShade(colNum);
                            }
                            if ((nvp[nvCount].value != "" && nvp[nvCount].name != "")) {
                                if (!args.layoutTable) {
                                    headTxt += " headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + " " +  getHeadId({id:'col', table:table_id, col:colNum}) + " " + idTxt + "'";
                                }
                            }
                            cellVal = nvp[nvCount].value;
                            if (cellVal == "") {
                                cellVal = "&nbsp;"
                            }
                            document.write("<td align='left' valign='top' class='" + cellClass + "'" + headTxt + ">\n");
                            document.write(cellVal + "\n");
                            document.write("<\/td>\n");
                        }
                    } else {
                        cellClass = "tableCell tableCellNoTop";
                        if (colNum == 0) {
                            cellClass += " tableCellNoLeft";
                        }
                        if (stripeColumns) {
                            cellClass += " " + getCellShade(colNum);
                        }
                        document.write("<td class='" + cellClass + "'>&nbsp;<\/td>\n");
                        if (nvp[0].value != undefined) {
                            cellClass = "tableCell tableCellNoTop tableCellNoLeft";
                            if (stripeColumns) {
                                cellClass += " " + getCellShade(colNum);
                            }
                            document.write("<td class='" + cellClass + "'>&nbsp;<\/td>\n");
                        }
                    }
                } else {
                    cellClass = "tableCell tableCellNoTop";
                    if (colNum == 0) {
                        cellClass += " tableCellNoLeft";
                    }
                    if (stripeColumns) {
                        cellClass += " " + getCellShade(colNum);
                    }
                    if (j == "status" && args.sampleHealth) {
                        document.write("<td class='" + cellClass + "'>" + args.sampleHealth + "<\/td>\n");
                        args.sampleHealth = "";
                    } else {
                       document.write("<td class='" + cellClass + "'>&nbsp;<\/td>\n");
                    }
                }
                colNum++;
            }
            document.write("<\/tr>");
        }
        if (args.summaryRow) {
            document.write("<tr" + rowClass + ">\n");
            document.write("<td class='tableCell tableCellNoTop tableCellNoLeft'>&nbsp;<\/td>\n");
            document.write("<td colspan='" + (numColumns-1) + "' class='tableCell'");
            if (!args.layoutTable) {
                document.write(" headers='" + getHeadId({id:'rowid', table:table_id, row:i}) + "'>" + tableArray[i]['data'][args.summaryRow] + "<\/td>\n");
            }
            document.write("<\/tr>");
        }
    }
    document.write("</tbody>\n");
    document.write("</table>\n");
    document.write("</form>\n");
    document.write("</div>\n");
    if (editableTbl) {
        for (i = 0; i < numRows; i++) {
            var action;
            if (tableArray[i]['meta']['settable'] == "true") {
                action = tableArray[i]['meta']['row_actions'];
                var row = tableArray[i]['data']['row_index'];
                var enabled_buttons = DefaultInit(tableArray[i]['meta']['enabled_buttons'], "ALL");
                ht_processTableClick({table_id:table_id, rowdata:row, row_actions:action, row:i, stripeRows:stripeRows, enabled_buttons:enabled_buttons});
                break;
            }
            var thisGroup = getHeadId({id:"row", table:table_id, row:i});
            if (tableArray[i]['data']['crows']) {
                var found = false;
                for (var j = 0; j < tableArray[i]['data']['crows'].length; j++) {
                    if (tableArray[i]['data']['crows'][j]['meta']['row_actions']) {
                        action = tableArray[i]['data']['crows'][j]['meta']['row_actions'];
                        var row = tableArray[i]['data']['crows'][j]['row_index']
                        var crow = thisGroup + "_sr" + j;
                        var enabled_buttons = DefaultInit(tableArray[i]['data']['crows'][j]['meta']['enabled_buttons'], "ALL");
                        ht_processTableClick({table_id:table_id, rowdata:row, row_actions:action, row:i, stripeRows:stripeRows, crow:crow, enabled_buttons:enabled_buttons});
                        ht_expandCRows(getElement(thisGroup));
                        found = true;
                        break;
                    }
                }
                if (found) {
                    break;
                }
            }
        }
    }
}
function clearFilters(elem_ary) {
    for (var i = 0; i < elem_ary.length; i++) {
        document.getElementById(elem_ary[i].id).value = "";
    }
}
function reloadFilteredTable(elem_ary,
                             numrows_elem,
                             filtermenu_elem,
                             base_url) {
    var default_numrows = 25;
    var min_numrows = 5;
    var Qstr = "";
    if (numrows_elem.value != "") {
        if (numrows_elem.value < min_numrows) {
            alert("The minimum number of rows per page is 5.");
            return false;
        }
        if (numrows_elem.value != default_numrows) {
            Qstr = Qstr.query_append("nrows", numrows_elem.value);
        }
    }
    var isFiltered = false;
    if (elem_ary) {
        for (var i = 0; i < elem_ary.length; i++) {
            var field_name = elem_ary[i].id;
            var elem = document.getElementById(field_name);
            if (elem.value != "") {
                Qstr = Qstr.query_append(field_name, encodeURIComponent(elem.value));
                isFiltered = true;
            }
        }
    }
    if (filtermenu_elem) {
        var selidx = (isFiltered ? filtermenu_elem.selectedIndex : 0);
        if (filtermenu_elem.options.length == (selidx + 1)) {
            if (filtermenu_elem.options[selidx - 1].disabled == false) {
                selidx--;
            }
        }
        Qstr = Qstr.query_append("filterSelIdx", selidx);
    }
    document.location.href = base_url + Qstr;
}
function ht_inlineAlert(type, title, msg, linktxt, linkurl, linktarget, onclick)
{
    document.write(ht_inlineAlertHtml(type, title, msg, linktxt, linkurl, linktarget, onclick));
}
function ht_getMsgIconHtml(type)
{
    switch (type.toLowerCase()) {
        case "error":
            tblIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/error_icon.png', width:12, height:13, alt:'Error', classname:'alertIcon', valign:'text-top'});
            break;
        case "warn":
            tblIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/warning_icon.png', width:13, height:13, alt:'Warning', classname:'alertIcon', valign:'text-top'});
            break;
        case "success":
            tblIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/okay_icon.png', width:12, height:13, alt:'Success', classname:'alertIcon', valign:'text-top'});
            break;
        case "info":
        default:
            tblIcon = showTransparentPngHtml({imgPath:'/Images/oracle/icons/attention_icon.png', width:12, height:13, alt:'Attention', classname:'alertIcon', valign:'baseline'});
    }
    return tblIcon;
}
function ht_inlineAlertHtml(type, title, msg, linktxt, linkurl, linktarget, onclick)
{
    var box = new String();
    var icon = ht_getMsgIconHtml(type);
    box += "<div align='center'>\n";
    box += "<table class='msgbox' role='presentation'><tr><td align='left' style='font-weight:bold'>" + icon;
    box += title + "</td></tr>";
    box += "<tr><td class='msgtxt' align='left'>" + msg;
    if (linktxt && (linkurl || onclick)) {
        box += "&nbsp;&nbsp;";
        box += "<a title='" + linktxt + "'";
        if (linkurl) { box += " href='" + linkurl + "'"; }
        if (onclick) { box += " onclick='" + onclick.safeQuote("'") + "'"; }
        if (linktarget) {
            box += " target='" + linktarget + "'";
            if (linktarget == "_blank") {
                linktxt = "(" + linktxt + "...)";
            }
        }
        box += ">" + linktxt + "</a>\n";
    }
    box += "</td></tr></table></div>";
    return box;
}
function ht_fieldAlert(type, msg, linktxt, linkurl, linktarget, onclick) {
    document.write(ht_fieldAlertHtml(type, msg, linktxt, linkurl, linktarget, onclick));
}
function ht_fieldAlertHtml(type, msg, linktxt, linkurl, linktarget, onclick) {
    var box = new String();
    tblIcon = ht_getMsgIconHtml(type);
    box += "<table border='0' cellpadding='0' cellspacing='0' role='presentation' class='msgbox' style='margin-bottom:10px'><tr>\n";
    box += "<td>" + tblIcon + "</td><td class='msgboxtxt'>";
    box += msg;
    if (linktxt && (linkurl || onclick)) {
        box += "  <a title='" + linktxt + "'";
        if (linkurl) { box += " href='" + linkurl + "'"; }
        if (onclick) { box += " onclick='" + onclick.safeQuote("'") + "'";}
        if (linktarget) {
            box += " target='" + linktarget + "'";
            if (linktarget = "_blank") {
                linktxt = "(" + linktxt + "...)";
            }
        }
        box += ">" + linktxt + "</a>\n";
    }
    box += "</td></tr></table>";
    return box;
}
function buildLicenseFunctionalityMsg(license, verb, functionality) {
    var msg = "A";
    if (license && license.length > 0) {
        if ("aeiou".contains(license.charAt(0).toLowerCase())) {
            msg = "An";
        }
        msg += " " + license;
    }
    msg += " license is required"
    if (functionality && functionality.length > 0 &&
        verb && verb.length > 0) {
        msg += " to " + verb + " " + functionality;
    }
    msg += ".";
    return msg;
}
function ht_updateOP() {
    if (parent && parent.treeFrame) {
        parent.treeFrame.updateOpenProblems();
    }
}
function ht_selectTreeNode(pri, sec, tab) {
    if (parent && parent.treeFrame) {
        parent.treeFrame.selectNode(undefined, pri, sec, tab);
    }
    return false;
}
function ht_inlineLicenseAlert(license, functionality)
{
    var msg = buildLicenseFunctionalityMsg(license, "use", functionality);
    var licenseASP = "i_licenses.asp";
    var jsTabSelect = "ht_selectTreeNode('System_Information', 'Licenses'); return false;";
    ht_inlineAlert("info", "ILOM License Required", msg, "Go to license configuration.", licenseASP, null, jsTabSelect);
}
function ht_fieldLicenseAlert(license, functionality) {
    var msg = buildLicenseFunctionalityMsg(license, "enable", functionality);
    var licenseASP = "i_licenses.asp";
    var jsTabSelect = "ht_selectTreeNode('System_Information', 'Licenses'); return false;";
    ht_fieldAlert("info", msg, "Go to license configuration.", licenseASP, null, jsTabSelect);
}
function setHelpText(helpTxt) {
    var helpDiv = document.getElementById("HelpDiv");
    if (helpDiv) {
        helpDiv.innerHTML = helpTxt;
    }
}
function ht_writePageHeader(args) {
    var helptext = DefaultInit(args.helpText, "");
    document.write("<div id='HelpDiv' class='helpText'>" + helptext + "<\/div>\n");
    if (args && args.conJump) {
        document.write("<div id='ConJumpDiv' class='conjumps'>\n");
        for (var i = 0; i < args.conJump.length; i++) {
            var ref = make_conjmp_href(args.conJump[i]);
            var focusRef;
            if (i == 0) {
                focusRef = "conjumpLink_" + (args.conJump.length - 1);
            } else {
                focusRef = ref + "_toplink";
            }
            document.write("<a href='javascript:giveFocus(\"" + focusRef + "\");gotoanchor(\"" + ref + "\");' class='conjumpLink' id='conjumpLink_" + i + "'>");
            showTransparentPng({imgPath:'/Images/oracle/icons/link_downArrow_icon.png', width:9, height:9,
                                alt:'Jump to ', classname:'healthIcon', valign:'baseline'});
            document.write(args.conJump[i] + "<\/a>");
        }
        document.write("<\/div>");
    }
    document.write("<hr class='divider' />");
    ht_pageSync();
}
function ht_writePopupHeader(args) {
    var helptext = DefaultInit(args.helpText, "");
    document.write("<div id='HelpDiv' class='popupHelpText'>" + helptext + "<\/div>\n");
}
function setTopFrameTitle(titleText, timeLeft)
{
    if (parent.topFrame == undefined || parent.topFrame.SetTitle == undefined) {
        if (timeLeft) {
            titleText = titleText.escapeQuotes();
            setTimeout("setTopFrameTitle('" + titleText + "'," + (timeLeft-1) + ")", 1000);
        }
    } else {
        parent.topFrame.SetTitle(titleText);
    }
}
function ht_setPopupTitle(titleText) {
    setTopFrameTitle(titleText, 10);
    if (parent.setPopupTitle) {
        parent.setPopupTitle(titleText);
    }
}
function parseURI(uri) {
    var proto = "";
    var user  = "";
    var pass  = "";
    var host  = "";
    var filepath = "";
    var tmpary = uri.split("://");
    if (tmpary.length != 2) { return null; }
    proto = tmpary[0];
    var tmp = tmpary[1];
    if (tmp.indexOf('@') > 0) {
        tmpary = tmp.split("@");
        if (tmpary.length != 2) { return null; }
        if (tmpary[0].indexOf(':') > 0) {
            var upary = tmpary[0].split(":");
            user = upary[0];
            pass = upary[1];
        } else {
            user = tmpary[0];
        }
        tmp = tmpary[1];
    }
    var x = tmp.indexOf(':');
    if (x < 0) {
        x = tmp.indexOf('/');
    }
    if (x < 0) {
        host = tmp;
    } else {
        host = tmp.slice(0,x);
        filepath = tmp.slice(x+1);
    }
    var info = {proto:proto, user:user, pass:pass, host:host, filepath:filepath};
    return info;
}
function ft_onchange(idprefix, cbfunc) {
    var protoelem = document.getElementById(idprefix + 'proto');
    var direlem = document.getElementById(idprefix + 'direction');
    var filerow = document.getElementById(idprefix + 'filerow');
    var browserrow = document.getElementById(idprefix + 'browserrow');
    var pasterow = document.getElementById(idprefix + 'pasterow');
    var hostrow = document.getElementById(idprefix + 'hostrow');
    var userrow = document.getElementById(idprefix + 'userrow');
    var localmediarow = document.getElementById(idprefix + 'localmediarow');
    var box = document.getElementById(idprefix + 'box');
    hideElement(filerow);
    hideElement(browserrow);
    hideElement(pasterow);
    hideElement(hostrow);
    hideElement(userrow);
    hideElement(localmediarow);
    var lastProto = protoelem.options[protoelem.length - 1];
    if (direlem && lastProto.value == "paste") {
        if (direlem.value == 'up') {
            ht_set_dropdown_option_state({obj:lastProto, state:'enabled'});
        } else {
            ht_set_dropdown_option_state({obj:lastProto, state:'disabled'});
        }
    }
    switch (protoelem.value) {
        case "browser":
            if (direlem) {
                if (direlem.value == 'up') {
                    showElement(filerow);
                } else {
                    showElement(browserrow);
                }
            }
            break;
        case "tftp":
            showElement(hostrow);
            break;
        case "ftp":
        case "ftps":
        case "scp":
        case "sftp":
        case "http":
        case "https":
            showElement(hostrow)
            showElement(userrow);
            break;
        case "media":
            showElement(localmediarow);
            break;
        case "paste":
            showElement(pasterow);
            break;
    }
    var cname = box.className;
    box.className = "";
    box.className = cname;
    if (typeof(cbfunc) == "function") {
        cbfunc();
    }
}
function fileTransferUsesPaste(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var pasterow = document.getElementById(idprefix + 'pasterow');
    return isElementVisible(pasterow);
}
function fileTransferUsesHost(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var hostrow = document.getElementById(idprefix + 'hostrow');
    return isElementVisible(hostrow);
}
function fileTransferUsesUsername(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var userrow = document.getElementById(idprefix + 'userrow');
    return isElementVisible(userrow);
}
function fileTransferUsesLocalMedia(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var localmediarow = document.getElementById(idprefix + 'localmediarow');
    return isElementVisible(localmediarow);
}
function fileTransferUsesBrowser(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var frow = document.getElementById(idprefix + 'filerow');
    var brow = document.getElementById(idprefix + 'browserrow');
    return isElementVisible(frow) || isElementVisible(brow);
}
function fileTransferUsesURI(_idprefix) {
    return (fileTransferUsesHost(_idprefix) ||
            fileTransferUsesLocalMedia(_idprefix));
}
function fileTransferUsesStreaming(_idprefix) {
    return (fileTransferUsesPaste(_idprefix) ||
            fileTransferUsesBrowser(_idprefix));
}
function fileTransferGetDirection(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "direction").value;
}
function fileTransferGetProtocol(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "proto").value;
}
function fileTransferGetHost(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "host").value;
}
function fileTransferGetFilepath(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "path").value;
}
function fileTransferGetUsername(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "user").value;
}
function fileTransferGetPassword(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "pass").value;
}
function fileTransferGetLocalMedia(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "local_media").value;
}
function fileTransferGetURI(_idprefix) {
    var proto = fileTransferGetProtocol(_idprefix);
    var uri = proto + "://";
    var filepath = fileTransferGetFilepath(_idprefix);
    var tmpstr;
    if (!fileTransferUsesURI(_idprefix)) {
        alert("Internal Error: fileTransferGetURI() called when non-URI protocol selected.");
        return;
    }
    if (proto == "media") {
        uri += fileTransferGetLocalMedia(_idprefix);
        return uri;
    }
    if (fileTransferUsesUsername(_idprefix) &&
        fileTransferGetUsername(_idprefix) != "") {
        tmpstr = userPwEscapeEncode(fileTransferGetUsername(_idprefix));
        uri += tmpstr + ":"
        tmpstr = userPwEscapeEncode(fileTransferGetPassword(_idprefix));
        uri += tmpstr + "@";
    }
    tmpstr = fileTransferGetHost(_idprefix);
    if (validIPv6(tmpstr)) {
        uri += "[" + tmpstr + "]";
    } else if (tmpstr.beginsWith("[") && tmpstr.contains("]")) {
        uri += tmpstr;
    } else {
        var tmpary = tmpstr.split(":");
        if (tmpary.length == 2) {
           tmpstr = encodeURIComponent(tmpary[0]) + ":" + encodeURIComponent(tmpary[1]);
        }
        else {
           tmpstr = encodeURIComponent(tmpstr);
        }
        tmpstr = String(tmpstr).replace(/@/g,"%40");
        uri += tmpstr;
    }
    if (filepath.length == 0) {
        uri += "/";
        return uri;
    }
    if (filepath[0] != '/') {
        uri += "/";
    }
    if (proto == "tftp" || proto == "ftp" || proto == "ftps") {
        if (filepath[0] == '/' && filepath[1] != '/') {
            uri += "/";
        }
    }
    uri += filepath;
    return uri;
}
function fileTransferGetUploadFile(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "file").value;
}
function fileTransferGetUploadPaste(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    return document.getElementById(idprefix + "paste").value;
}
function fileTransferSubmitFileUpload(args, _idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var uploadType;
    if (!fileTransferUsesStreaming(idprefix)) {
        alert("Internal Error: fileTransferSubmitFileUpload() called when URI protocol selected.");
        return;
    }
    if (fileTransferUsesBrowser(idprefix)) {
        uploadType = "file";
    } else if (fileTransferUsesPaste(idprefix)) {
        uploadType = "paste";
    } else {
        alert("Internal Error: Unknown file submit type.");
        return;
    }
    document.getElementById(idprefix + uploadType).name = args.filename;
    var uploadform = document.getElementById(idprefix + uploadType + "form");
    uploadform.action = args.action;
    if (args.target) {
        uploadform.target = args.target;
    }
    uploadform.submit();
}
function fileTransferErrorCheck(_idprefix) {
    var idprefix = DefaultInit(_idprefix, "htxfr_");
    var alertmsg = "";
    var setfocus = "";
    if (fileTransferUsesBrowser(idprefix)) {
        if ("up" == fileTransferGetDirection(idprefix) &&
            "" == fileTransferGetUploadFile(idprefix)) {
            alertmsg += "Please specify a local file to upload.\n";
            setfocus = DefaultInit(setfocus, idprefix + "file");
        }
    } else if (fileTransferUsesPaste(idprefix)) {
        if ("" == fileTransferGetUploadPaste(idprefix)) {
            alertmsg += "Please paste content to upload.\n";
            setfocus = DefaultInit(setfocus, idprefix + "paste");
        }
    } else if (fileTransferUsesLocalMedia(idprefix)) {
    } else {
        var host = fileTransferGetHost(idprefix);
        if (host == "") {
            alertmsg += "Please specify the remote hostname or IP address.\n";
            setfocus = DefaultInit(setfocus, idprefix + "host");
        }
        if (!validIpOrDomainName(host)) {
            alertmsg += "Please specify a valid hostname or IP address.\n";
            setfocus = DefaultInit(setfocus, idprefix + "host");
        }
        if (fileTransferGetFilepath(idprefix) == "") {
            alertmsg += "Please specify the file location path.\n";
            setfocus = DefaultInit(setfocus, idprefix + "path");
        }
        if (fileTransferUsesUsername(idprefix) &&
            fileTransferGetProtocol(idprefix) != "http" &&
            fileTransferGetProtocol(idprefix) != "https") {
            if (fileTransferGetUsername(idprefix) == "") {
                alertmsg += "A username is required for the chosen transfer method.\n";
                setfocus = DefaultInit(setfocus, idprefix + "user");
            }
            if (fileTransferGetPassword(idprefix) == "") {
                alertmsg += "A password is required for the chosen transfer method.\n";
                setfocus = DefaultInit(setfocus, idprefix + "pass");
            }
        }
    }
    if (alertmsg == "") {
        return null;
    }
    return {alertmsg:alertmsg, setfocus:setfocus};
}
function showFileTransfer(args) {
    var list      = DefaultInit(args.list, "browser, paste");
    var medialist = typeof(ht_getMediaList) == "function" ? ht_getMediaList() : "";
    var uriInfo   = parseURI(DefaultInit(args.uri, "://"));
    var idprefix  = DefaultInit(args.idprefix, "htxfr_");
    var direction = DefaultInit(args.direction, "down");
    var uptxt     = DefaultInit(args.uptxt, "Upload");
    var downtxt   = DefaultInit(args.downtxt, "Download");
    var fileordir = DefaultInit(args.fileordir, "file");
    var cbname    = args.onchangename;
    var cbfunc    = args.onchangefunc;
    var filepathstr = (fileordir == "dir" ? 'Dirpath' : 'Filepath');
    var onChangestr = 'ft_onchange(\"' + idprefix + '\"';
    if (typeof(cbname) != "undefined" && cbname.length > 0) {
        onChangestr += ', ' + cbname;
    }
    onChangestr += ');';
    document.write("<div id='" + idprefix + "box' class='ilom-inline-block' border='0' style='border:thin solid rgb(200,200,200); margin:5px 0px 5px 18px; padding-right:15px;'>\n");
    document.write("<table border='0' cellpadding='0' role='presentation'>\n");
    document.write(" <tr>\n");
    if (direction == "both") {
        document.write("  <td class='subtitle' align='right'>\n");
        document.write("   <label for='" + idprefix + "direction'>\n");
        document.write("   Operation:</label>\n");
        document.write("  </td>\n");
        document.write("  <td class='tblSpace' colspan='3'>\n");
        var ddary = new Array();
        ddary.push({value:'down', text:downtxt});
        ddary.push({value:'up', text:uptxt});
        showAsDropdown({id:idprefix + 'direction', ary:ddary, onChange:onChangestr});
        document.write("  </td>\n");
    } else {
        document.write("  <td class='subtitle' colspan='4'>\n");
        var txt = direction == "up" ? uptxt : downtxt;
        if (txt.trim() != "") {
            document.write("   <span>" + txt + "</span>\n");
        }
        document.write("   <input type='hidden' id='" + idprefix + "direction' value='" + direction + "' />\n");
        document.write("  </td>\n");
    }
    document.write(" </tr>\n");
    document.write(" <tr>\n");
    document.write("  <td class='subtitle' align='right'>\n");
    document.write("   <label for='" + idprefix + "proto'>\n");
    document.write("   Transfer Method:</label>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace'>\n");
    if (!args.ary) {
        if (list.indexOf("browser") == -1) {
            list = "browser, " + list;
        }
        if (direction != "down" && list.indexOf("paste") == -1) {
            list += ", paste";
        }
        args.ary = list.split(',');
    }
    var ddary = new Array();
    for (var i=0; i<args.ary.length; i++) {
        var val = "";
        var txt = "";
        args.ary[i] = args.ary[i].trim();
        var state = (uriInfo.proto == args.ary[i] || args.method == args.ary[i]) ? "selected" : "enabled";
        switch (args.ary[i]) {
            case "browser":
                val = "browser"; txt = "Browser"; break;
            case "tftp":
                val = "tftp"; txt = "TFTP"; break;
            case "ftp":
                val = "ftp"; txt = "FTP"; break;
            case "ftps":
                val = "ftps"; txt = "FTPS"; break;
            case "scp":
                val = "scp"; txt = "SCP"; break;
            case "sftp":
                val = "sftp"; txt = "SFTP"; break;
            case "http":
                val = "http"; txt = "HTTP"; break;
            case "https":
                val = "https"; txt = "HTTPS"; break;
            case "media":
                if (medialist.length == 0) {
                    state = "disabled";
                }
                txt = ht_getPlatformName() + " Local Device";
                val = "media";
                break;
            case "paste":
                val = "paste"; txt = "Paste"; break;
            default:
                val = args.ary[i]; txt = "UNKNOWN: " + args.ary[i]; break;
        }
        ddary.push({value:val, text:txt, state:state});
    }
    showAsDropdown({id:idprefix + 'proto', ary:ddary, onChange:onChangestr});
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "filerow' style='display:none;'>\n");
    document.write("  <td class='subtitle' align='right'>\n");
    document.write("   <label for='" + idprefix + "file'>\n");
    document.write("   Select File:</label>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace' colspan='3'>\n");
    document.write("  <form id='" + idprefix + "fileform' action='#' \n");
    document.write("        enctype='multipart/form-data' method='post'>\n");
    document.write("   <input class='textFld' type='file' size='50' name='" + idprefix + "file' id='" + idprefix + "file' />\n");
    document.write("  </form>\n");
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "browserrow' style='display:none;'>\n");
    document.write("  <td>&nbsp;</td>\n");
    document.write("  <td class='tblSpace' colspan='3'>\n");
    document.write("   <span class='text'>\n");
    document.write("   The downloaded file will be saved according to your browser settings.</span>\n");
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "pasterow' style='display:none;'>\n");
    document.write("  <td class='subtitle' valign='top' align='right'>\n");
    document.write("   <span><label for='" + idprefix + "paste'>\n");
    document.write("   Data:</label></span>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace' colspan='3'>\n");
    document.write("  <form id='" + idprefix + "pasteform' action='#' \n");
    document.write("        enctype='multipart/form-data' method='post'>\n");
    document.write("  <textarea class='textArea' id='" + idprefix + "paste' name='" + idprefix + "paste'\n");
    document.write("            rows='10' cols='80' class='TxtAra' aria-describedby='" + idprefix + "PasteHelp'></textarea>\n");
    document.write("  <div class='HlpFldTxt' id='" + idprefix + "PasteHelp'>\n");
    document.write("    Copy/paste content.\n");
    document.write("  </div>\n");
    document.write("  </form>\n");
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "hostrow' valign='top' style='display:none;'>\n");
    document.write("  <td class='subtitle' valign='top' align='right'>\n");
    document.write("   <label for='" + idprefix + "host'>\n");
    document.write("   Host:</label>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace' valign='top'>\n");
    document.write("   <input class='textFld' type='text' size='20' maxlength='255' name='" + idprefix + "host'\n");
    document.write("          id='" + idprefix + "host' value='" + uriInfo.host + "' aria-describedby='" + idprefix + "HostHelp' />\n");
    document.write("   <div class='HlpFldTxt' id='" + idprefix + "HostHelp'>IP Address or Hostname</div>\n");
    document.write("  </td>\n");
    document.write("  <td class='labelText' valign='top' align='right'>\n");
    document.write("   <label for='" + idprefix + "path'>\n");
    document.write("   " + filepathstr + ":</label>\n");
    document.write("  </td>\n");
    document.write("  <td class='txtFldSpace'>\n");
    document.write("   <input class ='textFld' type='text' size='50' maxlength='128' name='" + idprefix + "path'\n");
    document.write("          id='" + idprefix + "path' value='" + uriInfo.filepath + "' />\n");
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "userrow' style='display:none;'>\n");
    document.write("  <td class='subtitle' align='right'>\n");
    document.write("   <span><label for='" + idprefix + "user'>\n");
    document.write("   Username:</label></span>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace' >\n");
    document.write("   <input class='textFld' type='text' size='20' maxlength='64' name='" + idprefix + "user'\n");
    document.write("          id='" + idprefix + "user' value='" + uriInfo.user + "' />\n");
    document.write("  </td>\n");
    document.write("  <td class='labelText' align='right'>\n");
    document.write("   <span><label for='" + idprefix + "pass'>\n");
    document.write("   Password:</label></span>\n");
    document.write("  </td>\n");
    document.write("  <td class='txtFldSpace' >\n");
    document.write("   <input class='textFld' type='password' size='20' maxlength='64' name='" + idprefix + "pass'\n");
    document.write("          id='" + idprefix + "pass' value='" + uriInfo.pass + "' />\n");
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write(" <tr id='" + idprefix + "localmediarow' style='display:none;'>\n");
    document.write("  <td class='subtitle' align='right'>\n");
    document.write("   <span>\n");
    document.write("     <label for='" + idprefix + "local_media'>\n");
    document.write("   Local Device:</label></span>\n");
    document.write("  </td>\n");
    document.write("  <td class='tblSpace'>\n");
    if (medialist.length > 0) {
        var media_ary = new Array();
        var media_ddary = new Array();
        media_ary = medialist.split(';');
        for (var i=0; i<media_ary.length; i++) {
            var state = "enabled";
            var device_ary = new Array();
            media_ary[i] = media_ary[i].trim();
            device_ary = media_ary[i].split(':');
            if (device_ary[0] == "thumbdrive")
                    state = "selected";
            media_ddary.push({value:device_ary[0], text:device_ary[1],
                    state:state});
        }
        showAsDropdown({id:idprefix + 'local_media', ary:media_ddary});
    }
    document.write("  </td>\n");
    document.write(" </tr>\n");
    document.write("</table>\n");
    document.write("</div>\n");
    ft_onchange(idprefix, cbfunc);
}
function htap_gettxtid(id) {
    return id;
}
function htap_gettxt(id) {
    return getElement(htap_gettxtid(id));
}
function htap_getchkid(id) {
    return id + "::chk::";
}
function htap_getchk(id) {
    return getElement(htap_getchkid(id));
}
function htap_getreadonlyid(id) {
    return id;
}
function htap_getreadonlyspan(id) {
    return getElement(htap_getreadonlyid(id));
}
function htap_onclick(id) {
    enableTextField(htap_gettxt(id), !htap_getchk(id).checked);
}
function htap_readonly(id) {
    var obj = htap_getreadonlyspan(id);
    return (obj && obj.nodeName.toLowerCase() == "span") ? true : false;
}
function htap_readwrite(id) {
    return !htap_readonly(id);
}
function showAutoPort(id, args) {
    if (!args) args = {};
    var readonly = DefaultInit(args.readonly, "false");
    var txtsize = DefaultInit(args.size, 20);
    if (!readonly.toBoolean()) {
        var idtxt = htap_gettxtid(id);
        var idchk = htap_getchkid(id);
        document.write("<table class='tblLayout' role='presentation'>\n");
        document.write("  <tr>\n");
        document.write("    <td>\n");
        document.write("      <input type='text' class='TxtFld' size='" + txtsize + "' maxlength='5' value=''");
        document.write(" id='" + idtxt + "' name='" + idtxt + "' onkeypress='return numbersonly(event);' />\n");
        document.write("    </td>\n");
        document.write("    <td style='width:6px'>\n");
        document.write("    </td>\n");
        document.write("    <td>\n");
        document.write("      <input type='checkbox' class='cb'");
        document.write(" name='" + idchk + "' id='" + idchk + "' onclick='htap_onclick(\"" + idtxt + "\");' aria-describedby='" + idchk + "AutoselectHelp' />\n");
        document.write("  <label for='" + idchk + "'>Autoselect</label>");
        document.write("    </td>\n");
        document.write("  </tr>\n");
        document.write("</table>\n");
        document.write("<div class='HlpFldTxt' id='" + idchk + "AutoselectHelp'>The default is: Autoselect (0)</div>");
    } else {
        var idreadonly = htap_getreadonlyid(id);
        document.write("<span id='" + idreadonly + "'></span>\n");
    }
}
function autoPortGetValue(id) {
    return (htap_getchk(id).checked ? "0" : htap_gettxt(id).value);
}
function autoPortSetValue(id, port) {
    if (htap_readwrite(id)) {
        htap_gettxt(id).value = port;
        htap_getchk(id).checked = (0 == port);
        htap_onclick(id);
    } else {
        htap_getreadonlyspan(id).innerHTML = (port == 0 ? "0 (Autoselect)" : port);
    }
}
function autoPortErrorCheck(id) {
    var alertmsg = "";
    var setfocus = "";
    var port = autoPortGetValue(id);
    if (!htap_getchk(id).checked) {
        if (port == "") {
            autoPortSetValue(id, 0);
        } else if (port != 0 && !validPort(port)) {
            alertmsg += "Please enter a valid port number.\n";
            setfocus = DefaultInit(setfocus, htap_gettxt(id));
        }
    }
    if (alertmsg == "") {
        return null;
    }
    return {alertmsg:alertmsg, setfocus:setfocus};
}
function htct_getchkid(id) {
    return id;
}
function htct_getchk(id) {
    return getElement(htct_getchkid(id));
}
function htct_gettxtid(id) {
    return id + "::txt::";
}
function htct_gettxt(id) {
    return getElement(htct_gettxtid(id));
}
function htct_getreadonlyid(id) {
    return id;
}
function htct_getreadonlyspan(id) {
    return getElement(htct_getreadonlyid(id));
}
function htct_getreadonlylabelid(id) {
    return id + "::lbl::";
}
function htct_getreadonlylabel(id) {
    return getElement(htct_getreadonlylabelid(id));
}
function htct_onclick(id) {
    enableTextField(htct_gettxt(id), htct_getchk(id).checked);
}
function htct_readonly(id) {
    var obj = htct_getreadonlyspan(id);
    return (obj && obj.nodeName.toLowerCase() == "span") ? true : false;
}
function htct_readwrite(id) {
    return !htct_readonly(id);
}
function showCondText(id, args) {
    if (!args) args = {};
    var readonly = DefaultInit(args.readonly, "false");
    var txtsize = DefaultInit(args.size, 5);
    var txtmaxlength = DefaultInit(args.maxlength, 10);
    var txtlabel = DefaultInit(args.label, "");
    var txthelp = DefaultInit(args.help, "");
    var txtdefault = DefaultInit(args.defaulttext, "Disabled (0)");
    var labelloc = DefaultInit(args.labellocation, "after");
    var numbersonly = DefaultInit(args.numbersonly, "true");
    if (!readonly.toBoolean()) {
        var idtxt = htct_gettxtid(id);
        var idchk = htct_getchkid(id);
        document.write("<table class='tblLayout' role='presentation'>\n");
        document.write("  <tr>\n");
        document.write("    <td>\n");
        document.write("      <input type='checkbox' name='" + idchk + "' id='" + idchk + "' class='cb' onclick='htct_onclick(\"" + idchk + "\");' />\n");
        document.write("    </td>\n");
        document.write("    <td>\n");
        document.write("      <label for='" + idchk + "'>Enabled</label>\n");
        document.write("    </td>\n");
        document.write("  </tr>\n");
        document.write("  <tr style='height:5px;'>\n");
        document.write("    <td></td><td></td>\n");
        document.write("  </tr>\n");
        document.write("  <tr>\n");
        document.write("    <td></td>\n");
        document.write("    <td> \n");
        if (txtlabel != "" && labelloc == "before") {
            document.write("      <span class='textFld'><label for='" + idtxt + "'>" + txtlabel + "</label></span>\n");
        }
        if (numbersonly == "true") {
            document.write("      <input class='TxtFld' type='text' id='" + idtxt + "' name='" + idtxt + "' size='" + txtsize + "' maxlength='" + txtmaxlength + "' onkeypress='return numbersonly(event);' aria-describedby='" + idtxt + "DefaultCondHelp' />\n");
        } else {
            document.write("      <input class='TxtFld' type='text' id='" + idtxt + "' name='" + idtxt + "' size='" + txtsize + "' maxlength='" + txtmaxlength + idtxt + "DefaultCondHelp' />\n");
        }
        if (txtlabel != "" && labelloc != "before") {
            document.write("      <span class='txtFldSpace'><label for='" + idtxt + "'>" + txtlabel + "</label></span>\n");
        }
        txthelp += " The default is: " + txtdefault;
        document.write("      <br />\n");
        document.write("      <div class='HlpFldTxt' id='" + idtxt + "DefaultCondHelp'>" + txthelp + "</div>\n");
        document.write("    </td>\n");
        document.write("  </tr>\n");
        document.write("</table>\n");
    } else {
        var idreadonly = htct_getreadonlyid(id);
        document.write("<span class='text' id='" + idreadonly + "'></span>");
        if (txtlabel != "") {
            document.write("<span id='" + htct_getreadonlylabelid(id) + "' class='text'>");
            document.write("&nbsp;" + txtlabel + "</span>");
        }
        document.write("\n");
    }
}
function condTextGetValue(id) {
    return (htct_getchk(id).checked ? htct_gettxt(id).value : "0");
}
function condTextSetValue(id, val) {
    if (htct_readwrite(id)) {
        htct_gettxt(id).value = val;
        htct_getchk(id).checked = (0 != val);
        htct_onclick(id);
    } else {
        if (val == 0 && typeof(val) == "number") {
            val = "0 (Disabled)";
            if (htct_getreadonlylabel(id)) {
                htct_getreadonlylabel(id).innerHTML = "";
            }
        } else if (val == "") {
            val = "Disabled";
            if (htct_getreadonlylabel(id)) {
                htct_getreadonlylabel(id).innerHTML = "";
            }
        }
        htct_getreadonlyspan(id).innerHTML = val;
    }
}
function condTextErrorCheck(id, args) {
    if (!args) args = {};
    var valmin = DefaultInit(args.min, 0);
    var valmax = DefaultInit(args.max, 0);
    var errmsg = DefaultInit(args.msg, "Error");
    var alertmsg = "";
    var setfocus = "";
    var val = condTextGetValue(id);
    if (htct_getchk(id).checked) {
        if (val == "") {
            condTextSetValue(id, 0)
        } else if (val != 0 && !validNumberRange(val, valmin, valmax)) {
            alertmsg += errmsg.appendNewline();
            setfocus = DefaultInit(setfocus, htct_gettxt(id));
        }
    }
    if (alertmsg == "") {
        return null;
    }
    return {alertmsg:alertmsg, setfocus:setfocus};
}
function htst_gettxtid(id) {
    return id;
}
function htst_gettxt(id) {
    return getElement(htst_gettxtid(id));
}
function htst_getbtnid(id) {
    return id + "::btn::";
}
function htst_getbtn(id) {
    return getElement(htst_getbtnid(id));
}
function htst_getreadonlyid(id) {
    return id;
}
function htst_getreadonlyspan(id) {
    return getElement(htst_getreadonlyid(id));
}
function htst_gettitle(id) {
    return eval(id + "__ht_title__");
}
function htst_getchange(id) {
    return eval(id + "__ht_change__");
}
function htst_getrevert(id) {
    return eval(id + "__ht_revert__");
}
function htst_getsavedval(id) {
    return eval(id + "__ht_saveval__");
}
function htst_setsavedval(id, value) {
    eval(id + "__ht_saveval__ = '" + value + "'");
}
function htst_onclick(id) {
    var txt = htst_gettxt(id);
    var btn = htst_getbtn(id);
    if (btn.value == "Change") {
        btn.value = "Revert";
        btn.title = htst_getrevert(id);
        setTextFieldEnable(txt);
        htst_setsavedval(id, txt.value);
        txt.value = "";
        txt.focus();
    } else if (btn.value == "Revert") {
        btn.value = "Change";
        btn.title = htst_getchange(id);
        txt.value = htst_getsavedval(id);
        setTextFieldDisable(txt);
    } else {
        alert("Internal Error: Unknown click state.\n");
    }
}
function htst_readonly(id) {
    var obj = htst_getreadonlyspan(id);
    return (obj && obj.nodeName.toLowerCase() == "span") ? true : false;
}
function htst_readwrite(id) {
    return !htst_readonly(id);
}
function showSecretText(id, args) {
    if (!args) args = {};
    var readonly = DefaultInit(args.readonly, "false");
    var txtpassword = DefaultInit(args.password, "true");
    var txtsize = DefaultInit(args.size, 20);
    var txttitle = DefaultInit(args.title, "");
    if (!readonly.toBoolean()) {
        var idtxt = htst_gettxtid(id);
        var idbtn = htst_getbtnid(id);
        var txtchange = "Change " + txttitle;
        var txtrevert = "Revert " + txtchange;
        document.write("<table class='tblLayout' role='presentation'>\n");
        document.write("  <tr>\n");
        document.write("    <td>\n");
        document.write("      <input type='password' class='TxtFldDis' size='" + txtsize + "' maxlength='255' value=''");
        document.write(" id='" + idtxt + "' name='" + idtxt + "' disabled='disabled' />\n");
        document.write("    </td>\n");
        document.write("    <td style='width:6px'>\n");
        document.write("    </td>\n");
        document.write("    <td>\n");
        ht_drawButton({btnclass:"button", id:idbtn, value:"Change", title:txtchange,
                       onclick:"htst_onclick('" + idtxt + "');"});
        document.write("    </td>\n");
        document.write("  </tr>\n");
        document.write("</table>\n");
        document.write("<script type='text/javascript'>\n");
        document.write("  var " + id + "__ht_title__='" + txttitle + "';");
        document.write("  var " + id + "__ht_change__='" + txtchange + "';");
        document.write("  var " + id + "__ht_revert__='" + txtrevert + "';");
        document.write("  var " + id + "__ht_saveval__='';");
        document.write("</script>\n");
    } else {
        var idreadonly = htst_getreadonlyid(id);
        document.write("<span class='text' id='" + idreadonly + "'></span>\n");
    }
}
function secretTextGetValue(id) {
    return htst_gettxt(id).value;
}
function secretTextSetValue(id, value) {
    if (htst_readwrite(id)) {
        htst_gettxt(id).value = value;
    } else {
        htst_getreadonlyspan(id).innerHTML = DefaultInnerHtml(value);
    }
}
function secretTextIsEnabled(id) {
    return !htst_gettxt(id).disabled;
}
function secretTextErrorCheck(id, args) {
    if (!secretTextIsEnabled(id)) return null;
    if (!args) args = {};
    var whitespace = DefaultInit(args.allowspaces, "false");
    var maxlength = DefaultInit(args.maxlength, 20);
    var alertmsg = "";
    var setfocus = "";
    var title = DefaultInit(htst_gettitle(id), "Value");
    var val = secretTextGetValue(id);
    if (val.length > maxlength) {
        alertmsg += title + " can not be over " + maxlength + " characters.\n";
        setfocus = DefaultInit(setfocus, htst_gettxt(id));
    }
    if (!whitespace.toBoolean()) {
        if (typeof(containsWhitespace) == "function") {
            if (containsWhitespace(val)) {
                alertmsg += title + " may not contain a space.\n";
                setfocus = DefaultInit(setfocus, htst_gettxt(id));
            }
        } else {
            alertmsg = "Internal Error: Calling page does not include validation.js";
        }
    }
    var allstars = /^\*+$/;
    if (secretTextIsEnabled(id) && allstars.test(val)) {
        alertmsg += title + " can not consist entirely of '*' characters.\n";
        setfocus = DefaultInit(setfocus, htst_gettxt(id));
    }
    if (alertmsg == "") {
        return null;
    }
    return {alertmsg:alertmsg, setfocus:setfocus};
}
function htdt_gettxtid(id) {
    return id;
}
function htdt_gettxt(id) {
    return getElement(htdt_gettxtid(id));
}
function htdt_getreadonlyid(id) {
    return id;
}
function htdt_getreadonlyspan(id) {
    return getElement(htdt_getreadonlyid(id));
}
function htdt_readonly(id) {
    var obj = htdt_getreadonlyspan(id);
    return (obj && obj.nodeName.toLowerCase() == "span") ? true : false;
}
function htdt_readwrite(id) {
    return !htdt_readonly(id);
}
function htdt_getdefval(id) {
    return eval(id + "__ht_defval__");
}
function showDefaultText(id, defval, args) {
    if (!args) args = {};
    var readonly = DefaultInit(args.readonly, "false");
    var txtsize = DefaultInit(args.size, 20);
    var txtmaxlength = DefaultInit(args.maxlength, Math.floor(txtsize/2));
    var txtlabel = DefaultInit(args.label, "");
    var txthelp = DefaultInit(args.help, "");
    if (!readonly.toBoolean()) {
        var idtxt = htdt_gettxtid(id);
        document.write("<table class='tblLayout' role='presentation'>\n");
        document.write("  <tr>\n");
        document.write("    <td>\n");
        document.write("<input class='TxtFld' type='text' id='" + idtxt + "' name='" + idtxt + "' size='" + txtsize + "' maxlength='" + txtmaxlength + "' value='' aria-describedby='" + idtxt + "DefaultTxtHelp' />\n");
        document.write("    </td>\n");
        if (txtlabel != "") {
            document.write("    <td class='txtFldSpace'>\n");
            document.write("      <label for='" + idtxt + "'>" + txtlabel + "</label>\n");
            document.write("    </td>\n");
        }
        document.write("  </tr>\n");
        document.write("    </table>\n");
        txthelp += " The default is: " + defval;
        document.write("<div class='HlpFldTxt' id='" + idtxt + "DefaultTxtHelp'>" + txthelp + "</div>\n");
        document.write("<script type='text/javascript'>\n");
        document.write("  var " + id + "__ht_defval__='" + defval + "';");
        document.write("</script>\n");
    } else {
        var idreadonly = htdt_getreadonlyid(id);
        document.write("<span class='text' id='" + idreadonly + "'></span>");
        if (txtlabel != "") {
            document.write("<span class='text'>" + "&nbsp;" + txtlabel + "</span>");
        }
        document.write("\n");
    }
}
function defaultTextGetValue(id) {
    var val = htdt_gettxt(id).value;
    if (val == "") {
        val = htdt_getdefval(id);
        defaultTextSetValue(id, val);
    }
    return val;
}
function defaultTextSetValue(id, val) {
    if (htdt_readwrite(id)) {
        htdt_gettxt(id).value = val;
    } else {
        htdt_getreadonlyspan(id).innerHTML = val;
    }
}
function userRole_DocConfiguration(args) {
    var force_RO       = 0;
    var none_option    = 0;
    var noroles_option = 0;
    var new_opts       = "";
    if (args.forceRO == "true") {
        force_RO = 1;
    }
    if (args.noneOption == "true") {
        none_option = 1;
    }
    if (args.noRolesOption == "true") {
        noroles_option = 1;
    }
    document.write("<table class='tblLayout' title='' role='presentation'>");
    document.write("  <tr>");
    document.write("    <td colspan='2'>");
    document.write("      <select class='menuStd' name='ext_role' id='ext_role' ");
    if (force_RO == 1) {
       new_opts = ", {forceRO:'true'}";
    }
    document.write("     onChange=\"userRole_ToggleBoxes('--onChange--'" +
                         new_opts + ");\">");
    document.write("       <option class='optText' value='leg_admin'>Administrator</option>");
    document.write("       <option class='optText' value='leg_oper'>Operator</option>");
    document.write("       <option class='optText' value='advanced'>Advanced Roles</option>");
    if (none_option == 1) {
       document.write("       <option class='optText' value='none'>None (server authorization)</option>");
    } else if (noroles_option == 1) {
       document.write("       <option class='optText' value='none'>None (no assigned roles)</option>");
    }
    document.write("      </select>");
    document.write("    </td>");
    document.write("  </tr>");
    document.write("  <tr>");
    document.write("    <td><input name='adminRole' type='checkbox' id='adminRole'");
    document.write("        class='cb' /> ");
    document.write("        <label for='adminRole'>Admin&nbsp;(a)</label> ");
    document.write("    </td>");
    document.write("    <td><input name='userMgmtRole' type='checkbox' ");
    document.write("         id='userMgmtRole' class='cb' />");
    document.write("        <label for='userMgmtRole'>User Management&nbsp;(u)</label>");
    document.write("    </td>");
    document.write("  </tr>");
    document.write("  <tr>");
    document.write("    <td><input name='consoleRole' type='checkbox' id='consoleRole'");
    document.write("        class='cb' />");
    document.write("        <label for='consoleRole'>Console&nbsp;(c)</label>");
    document.write("    </td>");
    document.write("    <td><input name='resetRole' type='checkbox' ");
    document.write("        id='resetRole' class='cb' />");
    document.write("        <label for='resetRole'>Reset and Host Control&nbsp;(r)</label>");
    document.write("    </td>");
    document.write("  </tr>");
    document.write("  <tr>");
    document.write("    <td><input name='readOnlyRole' type='checkbox' id='readOnlyRole'");
    document.write("         class='cbDis' disabled='true' checked='true' />");
    document.write("         <label for='readOnlyRole'>Read Only&nbsp;(o)</label>");
    document.write("    </td>");
    document.write("    <td><input name='serviceRole' type='checkbox' ");
    document.write("         id='serviceRole' class='cb' />");
    document.write("         <label for='serviceRole'>Service&nbsp;(s)</label>");
    document.write("    </td>");
    document.write("  </tr>");
    document.write("</table>");
}
userRole_ToggleBoxes = function(role_bits, options) {
    var adminCheck     = document.getElementById("adminRole");
    var userCheck      = document.getElementById("userMgmtRole");
    var consoleCheck   = document.getElementById("consoleRole");
    var resetCheck     = document.getElementById("resetRole");
    var serviceCheck   = document.getElementById("serviceRole");
    var readCheck      = document.getElementById("readOnlyRole");
    var selectedItem   = document.getElementById("ext_role");
    var force_RO       = 0;
    var none_option    = 0;
    var noroles_option = 0;
    if (options && options.forceRO == "true") {
        force_RO = 1;
    }
    if (options && options.noneOption == "true") {
        none_option = 1;
    }
    if (options && options.noRolesOption == "true") {
        noroles_option = 1;
    }
    if (role_bits == "--onChange--") {
       role_bits = "";
    } else if (role_bits == "Administrator") {
       selectedItem.value = "leg_admin";
    } else if (role_bits == "Operator") {
       selectedItem.value = "leg_oper";
    } else if (role_bits == "" && none_option == 1) {
       selectedItem.value = "none";
    } else if (role_bits == "" && noroles_option == 1) {
       selectedItem.value = "none";
    } else {
       selectedItem.value = "advanced";
    }
    if (selectedItem.value != "advanced" ) {
        setCheckboxDisable(adminCheck);
        setCheckboxDisable(userCheck);
        setCheckboxDisable(consoleCheck);
        setCheckboxDisable(resetCheck);
        setCheckboxDisable(serviceCheck);
        setCheckboxDisable(readCheck);
        if (selectedItem.value == "leg_admin") {
            adminCheck.checked   = true;
            consoleCheck.checked = true;
            resetCheck.checked   = true;
            userCheck.checked    = true;
            serviceCheck.checked = false;
            readCheck.checked    = true;
        } else if (selectedItem.value == "leg_oper") {
            consoleCheck.checked = true;
            resetCheck.checked   = true;
            adminCheck.checked   = false;
            userCheck.checked    = false;
            serviceCheck.checked = false;
            readCheck.checked    = true;
        } else if (selectedItem.value == "none") {
            consoleCheck.checked = false;
            resetCheck.checked   = false;
            adminCheck.checked   = false;
            userCheck.checked    = false;
            serviceCheck.checked = false;
            readCheck.checked    = false;
        } else {
            alert("userRole_ToggleBoxes: unexpected sel-value '" + selectedItem.value + "'");
        }
     }
     else {
        setCheckboxEnable(adminCheck);
        adminCheck.checked = role_bits.match("a");
        setCheckboxEnable(userCheck);
        userCheck.checked = role_bits.match("u");
        setCheckboxEnable(consoleCheck);
        consoleCheck.checked = role_bits.match("c");
        setCheckboxEnable(resetCheck);
        resetCheck.checked = role_bits.match("r");
        setCheckboxEnable(serviceCheck);
        serviceCheck.checked = role_bits.match("s");
        if (force_RO == 0) {
           setCheckboxEnable(readCheck);
           readCheck.checked = role_bits.match("o");
        } else {
           setCheckboxDisable(readCheck);
           readCheck.checked = true;
        }
     }
}
userRole_ExpandValue = function(defaultrole) {
    var expanded_string = "";
    if (defaultrole == "") {
        expanded_string = DefaultInnerHtml("");
    } else if (defaultrole.match("empty_table_cell")) {
        expanded_string = DefaultInnerHtml("");
    } else if  (defaultrole == "Administrator" || defaultrole == "Operator") {
        expanded_string = defaultrole;
    } else {
      var roleArray = new Array();
      if (defaultrole.match("a"))
         { roleArray.push("Admin"); }
      if (defaultrole.match("u"))
         { roleArray.push("User Management"); }
      if (defaultrole.match("c"))
         { roleArray.push("Console"); }
      if (defaultrole.match("r"))
         { roleArray.push("Reset and Host Control"); }
      if (defaultrole.match("o"))
         { roleArray.push("Read Only"); }
      if (defaultrole.match("s"))
         { roleArray.push("Service"); }
      expanded_string = roleArray.join(", ") + " (" + defaultrole + ")";
  }
   return expanded_string;
}
userRole_CalcSetValue = function() {
   var roles    = "";
   var set_mode = "";
   var roleSelection = document.getElementById("ext_role").value;
   if (roleSelection == "leg_admin") {
       set_mode = "leg-admin";
       roles = "Administrator";
   } else if (roleSelection == "leg_oper") {
       set_mode = "leg-oper";
       roles = "Operator";
   } else {
       set_mode = "advanced-roles";
       if (document.getElementById("adminRole").checked) {
           roles += "a";
       }
       if (document.getElementById("userMgmtRole").checked) {
           roles += "u";
       }
       if (document.getElementById("consoleRole").checked) {
           roles += "c";
       }
       if (document.getElementById("resetRole").checked) {
           roles += "r";
       }
       if (document.getElementById("serviceRole").checked) {
           roles += "s";
       }
       if (document.getElementById("readOnlyRole").checked) {
           roles += "o";
       }
   }
   return roles;
}
userRole_ValidateRoles = function() {
   var roles      = "";
   var err_string = "";
   var roleSelection = document.getElementById("ext_role").value;
   if (roleSelection == "advanced") {
       if (document.getElementById("adminRole").checked) {
           roles += "a";
       }
       if (document.getElementById("userMgmtRole").checked) {
           roles += "u";
       }
       if (document.getElementById("consoleRole").checked) {
           roles += "c";
       }
       if (document.getElementById("resetRole").checked) {
           roles += "r";
       }
       if (document.getElementById("serviceRole").checked) {
           roles += "s";
       }
       if (document.getElementById("readOnlyRole").checked) {
           roles += "o";
       }
       if (roles.length == 0) {
          err_string = "Advanced mode requires a role setting. Use None " +
                       "to clear all of the roles.";
       }
   }
   return err_string;
}
function ht_drawSubSectTitle(title) {
    document.write(ht_SubSectTitleHtml(title));
}
function ht_SubSectTitleHtml(title) {
    var htmlsrc = "<table border='0' cellpadding='0' cellspacing='10' title='' role='presentation'>\n";
    htmlsrc += "  <tr>\n";
    htmlsrc += "    <td><h1 class='title'>" + title + "<\/h1><\/td>\n";
    htmlsrc += "  <\/tr>\n";
    htmlsrc += "<\/table>\n";
    return htmlsrc;
}
function ht_drawButton(args) {
    document.write(ht_ButtonHtml(args));
}
function ht_ButtonHtml(args) {
    var btnclass = DefaultInit(args.btnclass, "tblButton");
    var id = DefaultInit(args.id, btnclass);
    var value = DefaultInit(args.value, id);
    var title = DefaultInit(args.title, value);
    var onclick = DefaultInit(args.onclick, "");
    var style = DefaultInit(args.style, "");
    id = id.make_html_id();
    var htmlsrc = "<input class='" + btnclass + "' type='submit' name='" + id + "'\n";
    htmlsrc += "id='" + id + "' value='" + value + "' title='" + title + "'\n";
    if (style != "") {
        htmlsrc += "style='" + style + "'\n";
    }
    htmlsrc += "onclick=\"" + onclick + "\" \/>\n";
    return htmlsrc;
}
function ht_drawRadio(name, argary, numcolumns) {
    document.write(ht_RadioHtml(name, argary, numcolumns));
}
function ht_RadioHtml(name, argary, numcolumns) {
    if (!name || !argary) {
        return null;
    }
    var numcols = DefaultInit(numcolumns, 1);
    var htmlsrc = "<table border='0' cellspacing='0' cellpadding='5' role='presentation'>\n";
    htmlsrc += "  <tr>\n";
    htmlsrc += "    <td>\n";
    for (var i = 0; i < argary.length; i++) {
        if (i>0) {
            htmlsrc += "    <\/td>\n";
            if (0 == i%numcols) {
                htmlsrc += "  <\/tr>\n";
                htmlsrc += "  <tr>\n";
            }
            htmlsrc += "    <td>\n";
        }
        var args = argary[i];
        var id = DefaultInit(args.id, name + i);
        var value = DefaultInit(args.value, id);
        var label = DefaultInit(args.label, value);
        var onclick = DefaultInit(args.onclick, "");
        id = id.make_html_id();
        htmlsrc += "<input class='rb' type='radio' name='" + name + "' id='" + id + "' value='" + value + "' onclick='" + onclick + "' \/>";
        htmlsrc += "<label for='" + id + "' class='text'>" + label + "<\/label>\n";
    }
    htmlsrc += "    <\/td>\n";
    htmlsrc += "  <\/tr>\n";
    htmlsrc += "<\/table>\n";
    return htmlsrc;
}
function ht_drawCheckbox(name, argary, numcolumns) {
    document.write(ht_CheckboxHtml(name, argary, numcolumns));
}
function ht_CheckboxHtml(name, argary, numcolumns) {
    if (!name || !argary) {
        return null;
    }
    var numcols = DefaultInit(numcolumns, 1);
    var htmlsrc = "<table border='0' cellspacing='0' cellpadding='5' role='presentation'>\n";
    htmlsrc += "  <tr>\n";
    htmlsrc += "    <td>\n";
    for (var i = 0; i < argary.length; i++) {
        if (i>0) {
            htmlsrc += "    <\/td>\n";
            if (0 == i%numcols) {
                htmlsrc += "  <\/tr>\n";
                htmlsrc += "  <tr>\n";
            }
            htmlsrc += "    <td>\n";
        }
        var args = argary[i];
        var id = DefaultInit(args.id, name + i);
        var value = DefaultInit(args.value, id);
        var label = DefaultInit(args.label, value);
        var onclick = DefaultInit(args.onclick, "");
        var checked = DefaultInit(args.checked, "").toBoolean() ? " checked='checked'" : "";
        var disabled = DefaultInit(args.disabled, "").toBoolean() ? " disabled='disabled'" : "";
        id = id.make_html_id();
        htmlsrc += "<input class='cb' type='checkbox' name='" + name + "' id='" + id +
            "' value='" + value + "'" + checked + disabled + " onclick='" + onclick + "' \/>";
        htmlsrc += "<label for='" + id + "' class='text'>" + label + "<\/label>\n";
    }
    htmlsrc += "    <\/td>\n";
    htmlsrc += "  <\/tr>\n";
    htmlsrc += "<\/table>\n";
    return htmlsrc;
}
function extractSlotFromNAC(nac, prefix) {
    var pfx = DefaultInit(prefix, "BL");
    var idx = nac.indexOf(pfx);
    if (idx == -1) {
        return -1;
    }
    var tmp = nac.slice(idx+pfx.length);
    return parseInt(tmp);
}
function make_pretty_propname(p) {
    p = p.replace(/[_-]/g, " ");
    p = p.replace(/^pendingip/, "Pending IP ");
    p = p.replace(/^pending/, "Pending ");
    p = p.replace(/macaddress/, " MAC Address ");
    p = p.replace(/outofband/, " Out of Band ");
    p = p.replace(/managementport/, " Management Port ");
    p = p.replace(/frudata/, " FRU Data ");
    p = p.replace(/sec average$/, " Second Average");
    p = p.replace_word("ipmi name", "IPMI Name");
    p = p.replace_word("sun msg id", "Message ID");
    p = p.replace_word("obj name", "Name");
    p = p.replace_word("sampleset", "Sample Set");
    p = p.replace_word("nonrecov", "Nonrecoverable");
    p = p.replace_word("uuid", "UUID");
    p = p.replace_word("uuids", "UUIDs");
    p = p.replace_word("wwn", "WWN");
    p = p.replace_word("wwns", "WWNs");
    p = p.replace_word("guid", "GUID");
    p = p.replace_word("guids", "GUIDs");
    p = p.replace_word("id", "ID");
    p = p.replace_word("ids", "IDs");
    p = p.replace_word("cpu", "CPU");
    p = p.replace_word("dimm", "DIMM");
    p = p.replace_word("mac", "MAC");
    p = p.replace_word("fru", "FRU");
    p = p.replace_word("pci", "PCI");
    p = p.replace_word("pcie", "PCIE");
    p = p.replace_word("raid", "RAID");
    p = p.replace_word("raids", "RAIDs");
    p = p.replace_word("ip", "IP");
    return p.collapse().trim().capitalize();
}
function change_modulename(name)
{
    name = name.replace(/[_-]/g, " ");
    return name.collapse().trim().capitalize();
}
function getMainFrame(ref)
{
    var topMainFrame;
    if (ref) {
        topMainFrame = ref.top.mainFrame;
    } else {
        topMainFrame = top.mainFrame;
    }
    if (topMainFrame && topMainFrame.mainFrame) {
        return topMainFrame.mainFrame;
    }
    if (topMainFrame) {
        return topMainFrame
    }
    return null;
}
function getDomainNameForHost(host)
{
    if (!host) {
        return "";
    }
    var domainName = host.replace("HOST", "Domain ");
    return domainName.replace("/", "");
}
function ht_hideNavigationLabel()
{
    if (parent.pageHeader && parent.pageHeader.hideNavigationLabel) {
        parent.pageHeader.hideNavigationLabel();
    }
}
function ht_showNavigationLabel()
{
    if (parent.pageHeader && parent.pageHeader.showNavigationLabel) {
        parent.pageHeader.showNavigationLabel();
    }
}
function ht_processTab(name)
{
    if (parent.mainFrame && parent.mainFrame.processTabByName) {
        parent.mainFrame.processTabByName(name);
    }
}
function ht_showTabs()
{
    if (parent.mainFrame && parent.mainFrame.showTabs) {
        parent.mainFrame.showTabs();
    }
}
function ht_hideTabs()
{
    if (parent.mainFrame && parent.mainFrame.hideTabs) {
        parent.mainFrame.hideTabs();
    }
}
function ht_hideNavTree()
{
    if (parent.treeFrame && parent.treeFrame.hideTree) {
        parent.treeFrame.hideTree();
    }
}
function ht_showNavTree()
{
    if (parent.treeFrame && parent.treeFrame.showTree) {
        parent.treeFrame.showTree();
    }
}
function ht_hideAllNavigation()
{
    ht_hideNavTree();
    ht_hideTabs();
    ht_hideNavigationLabel();
}
function ht_showAllNavigation()
{
    ht_showNavTree();
    ht_showTabs();
    ht_showNavigationLabel();
}
function ht_setTitleBar(val) {
    if (parent.pageHeader && parent.pageHeader.setTitleBar) {
        parent.pageHeader.setTitleBar(val);
    }
}
function ht_pageSync()
{
     if (parent.treeFrame && parent.treeFrame.syncTreeAndTabs) {
         parent.treeFrame.syncTreeAndTabs(this.document.location.pathname + this.document.location.search);
     }
}
function ht_drawTabs(args) {
    if (parent.mainFrame && parent.mainFrame.drawTabs) {
        parent.mainFrame.drawTabs(args);
    }
}
function ht_getFullName(args)
{
    if (args.longname != undefined) {
        return args.longname;
    }
    return args.name;
}
function setStatusPage(setstatus)
{
    return ("statusPage.asp?setstatus=" + setstatus);
}
function ht_setCursor(cursor_style) {
    var style_item = " " + cursor_style;
    if (top.opener) {
        top.opener.document.body.className += style_item;
    }
    if (parent.bottomFrame) {
        parent.bottomFrame.document.body.className += style_item;
    }
    document.body.className += style_item;
}
function ht_unsetCursor(cursor_style) {
    var re = new RegExp("(^|\\s)" + cursor_style + "(?!\\S)", "g");
    if (top.opener) {
        top.opener.document.body.className = top.opener.document.body.className.replace(re, "");
    }
    if (parent.bottomFrame) {
        parent.bottomFrame.document.body.className = parent.bottomFrame.document.body.className.replace(re, "");
    }
    document.body.className = document.body.className.replace(re, "");
}
function ht_setCursorWait() {
    ht_setCursor("wait");
}
function ht_unsetCursorWait() {
    ht_unsetCursor("wait");
}
